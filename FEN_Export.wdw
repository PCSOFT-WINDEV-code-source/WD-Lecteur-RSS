     internal_properties : CAAAAAgAAABmqF6cMJ3/KZ+8LLQbLPMfLKAjqgIxKzXp7P5DC0XQjAg5XY42VSv/dylD3LlGVBa1XLSZ9xAhG4pUaz5ZPOwuozG1zXA5xF1FuiTQEuGZH4EzajrxePzI+8w53mVsQV+svobcWKEUEKWseTEEXtq2fpEW2eZX570TnDH23WNcNHF2r2tjCv6c47TcX5qQRwLDj0YHPw==
     procedure_id : 1559300285652334862
     type_code : 14
     code : |1-
      // Résumé : Coche / Décoche noeud
      // Syntaxe :
      //CocheNoeuds ( [<bCoché> est booléen])
      //
      // Paramètres :
      //	bCoché (booléen - valeur par défaut=1) : Valeur de la coche
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE CocheNoeuds(LOCAL bCoché est un booleen = vrai)
      
      
      // Parcourt les dossiers
      sDossier est une chaine = ArbreRécupèreFils(ARBRE_Export, "", aPremier)
      TANTQUE sDossier <> ""
      	
      	// Coche le dossier
      	ARBRE_Export[sDossier]..Cochée = bCoché
      	
      	// Parcourt les flux
      	sFlux est une chaine = ArbreRécupèreFils(ARBRE_Export, sDossier, aPremier)
      	TANTQUE sFlux <> ""
      		
      		// Coche le flux
      		ARBRE_Export[sDossier+TAB+sFlux]..Cochée = bCoché
      	
      		// Flux suivant	
      		sFlux = ArbreRécupèreFils(ARBRE_Export, sDossier, aSuivant)
      		
      	FIN
      	
      	// Dossier suivant
      	sDossier = ArbreRécupèreFils(ARBRE_Export, "", aSuivant)
      	
      FIN
      	
     type : 458752
   -
     name : RemplitDossier
     internal_properties : CAAAAAgAAAAmUcIUBjps+ZQka2m2CHOqRTIesuYlgQZusNBJOY7ix7Cte8o5iMS53obSUHbUIZo+DgYbHqQxg74a8Wbs+gy9dQZwJJAsm64kTK5dJfJ54pcE5LKWu4Af7YJd7UiVDahUgxSIvR7iHREMBzj5MRxFyoiea6WuSe7h3r2JbobGtyP9pXf010Yj2fZY4MXBpAak0GsCHbY=
     procedure_id : 1559301578439036907
     type_code : 14
     code : |1+
      // Résumé : Remplit les dossiers
      // Syntaxe :
      //RemplitDossier ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE RemplitDossier()
      
      // Vide l'arbre
      ArbreSupprimeTout(ARBRE_Export)
      
      // Abonnements
      arbreajoute(ARBRE_Export, <§$0005§>, IMG_RépertoireRSS, IMG_RépertoireRSS, 0)
      ARBRE_Export[<§$0005§>]..Cochée = Vrai 
      
      // Ajoute les dossiers
      pour TOUT dossier sur Libellé
      	arbreajoute(ARBRE_Export, <§$0005§> +tab+ dossier.libellé, IMG_Répertoire_Fermé, IMG_Répertoire_Fermé, dossier.IDDossier)	
      	// Coche le noeud
      	ARBRE_Export[<§$0005§> +TAB+ Dossier.Libellé]..Cochée = Vrai 
      FIN
      
      // Déroule l'arbre
      arbrederouletout(ARBRE_Export)
      
