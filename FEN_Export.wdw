  -
    name : BTN_Exporter
    identifier : 0x25a3bf9211c3d5a2
    internal_properties : 
    properties :
     x : 432
     y : 385
     width : 100
     height : 30
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        internal_properties : 
        code : |1+
         bAuMoinsUneCoche 	est un booleen = faux
         nIndiceDossier		est un entier
         nIndiceFlux			est un entier
         sListeExportée 		est une chaine
         sExport 			est une chaine
         
         sModele est une chaine = [
         <opml version="1.1">
         <head>
         <title>%1</title>
         <dateModified>%2</dateModified>
         </head>
         <body>
         %3
         </body>
         </opml>
         ]
         
         // Si le dossier "Abonnements" est coché
         si ARBRE_Export[<§$0005§>]..Cochée ALORS
         	// Exporte les flux sans dossier
         	POUR TOUT FluxRSS AVEC IDDossier = 0
         		bAuMoinsUneCoche = vrai
         		sListeExportée += [RC] + ChaîneConstruit("<outline text=""%1"" xmlUrl=""%2""/>", Remplace(ChaîneVersUTF8(FluxRSS.Titre), "&", "amp;"), FluxRSS.Adresse)
         	FIN
         FIN
         
         // Parcourt les sous-dossiers
         sDossier est une chaine = ArbreRécupèreFils(ARBRE_Export, <§$0005§>, aPremier)
         TANTQUE sDossier <> ""
         	
         	// Si le dossier est coché
         	si ARBRE_Export[<§$0005§> +tab+ sDossier]..Cochée alors
         		
         		// Mémorise l'identifiant dans la liste
         		si ArbreRécupèreIdentifiant(ARBRE_Export, <§$0005§> +TAB+ sDossier) > 0 alors
         			// Au moins un dossier est coché
         			bAuMoinsUneCoche = vrai
         			nIndiceDossier++
         			
         			// Lit le dossier
         			hlitrecherchepremier(dossier, iddossier, ArbreRécupèreIdentifiant(ARBRE_Export, <§$0005§> +TAB+ sDossier))
         			si htrouve(dossier) = faux alors continuer
         			
         			// Libellé du dossier
         			sListeExportée += [RC] + chaineconstruit("<outline text=""%1"">", Remplace(ChaîneVersUTF8(dossier.libellé), "&", "amp;"))
         									
         			// Parcourt les flux
         			nIndiceFlux = 1
         			pour TOUT FluxRSS avec IDDossier = dossier.IDDossier
         				sListeExportée += [RC] + ChaîneConstruit("<outline text=""%1"" xmlUrl=""%2""/>", Remplace(ChaîneVersUTF8(FluxRSS.Titre), "&", "amp;"), fluxrss.adresse)
         			FIN
         			sListeExportée += [RC] + "</outline>"
         		Fin
         		
         	FIN
         	
         	// Dossier suivant
         	sDossier = ArbreRécupèreFils(ARBRE_Export, <§$0005§>, aSuivant)
         	
         FIN
         
         // Si aucun dossier coché
         si bAuMoinsUneCoche = faux ALORS
         	erreur(<§$0003§>)
         	reprisesaisie(ARBRE_Export)
         FIN
         
         // Construit l'export
         sExport = ChaîneConstruit(sModele, <§$0004§>, DateVersChaîne(DateDuJour()+Maintenant(),maskDateEmail), sListeExportée)
         
         // Ouvre le sélecteur de fichier
         sFichier est une chaîne
         sFichier = fSélecteur("", "", <§$0001§>, <§$0002§> + TAB + "*.opml", "*.opml", fselCrée + fselExiste)
         // Si un fichier est sélectionné
         si sFichier ~= "" alors retour
         
         // Mémorise le buffer dans le fichier XML
         fsauvetexte(sFichier, sExport)
         
         // Ouvre le répertoire dans l'explorateur Windows
         lanceappliassociée(fExtraitChemin(sFichier, fDisque+fRépertoire))
         
         // Message de fin de traitement
         toastaffiche(<§$0000§>, toastcourt, cvMilieu, chcentre)
         
         // Ferme la fenêtre
         Ferme()
         
        type : 18
