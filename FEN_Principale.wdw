     name : Tags_Coche
     procedure_id : 1591930762144327343
     type_code : 14
     code : |1-
      // Résumé : Procédure appelée lorsqu'un tag est cochée / décochée
      // Syntaxe :
      //Tags_Coche (<nIDTag> est entier sur 8 octets)
      //
      // Paramètres :
      //	nIDTag (entier sur 8 octets) : Identifiant du tag
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE Tags_Coche(LOCAL nIDTag est un entier sur 8)
      
      // Mémorise l'identifiant du billet
      nIDBillet est un entier sur 8 = gnIDBilletCourant
      SI gnIDBilletCourant <= 0 ALORS RETOUR
      
      // Ajoute ou supprime l'association
      HLitRecherchePremier(Association,IDAssociation,[nIDBillet, nIDTag])
      si htrouve(association) ALORS
      	hsupprime(association)
      SINON
      	hraz(association)
      	association.IDBillet = nIDBillet
      	association.idtag = nIDTag
      	hajoute(association)
      FIN
      
      // Met à jour le compteur pour ce tag
      MAJ_Compteur_Tag(nIDTag)
      ComptageApplique_UnIdentifiant(PREFIX_TAG+nIDTag, Faux)
      
      // Rafraîchit l'affichage de la ligne
      nIndice est un entier
      sTagsAssociés est une chaine = Billet_ListeTagsAssociés(gnIDBilletCourant)
      
      nIndice = TableCherche(COL_IDBillet, gnIDBilletCourant)
      SI nIndice > 0 ALORS COL_TagsAssociés[nIndice] = sTagsAssociés ; tableaffiche(TABLE_Billets, nIndice)
      
      nIndice = ZoneRépétéeCherche(ATT_IDBillet, gnIDBilletCourant)
      SI nIndice > 0 ALORS ATT_TagsAssociés[nIndice] = sTagsAssociés ; zonerepeteeAffiche(ZR_Billets, nIndice)
