     name : AfficheBoutonsAffichageZR
     procedure_id : 1594473803728547875
     type_code : 14
     code : |1+
      // Résumé : Affiche / Cache les boutons d'actions de l'affichage en mode ZR
      // Syntaxe :
      //AfficheBoutonsAffichageZR ( [<bVisible> est booléen [, <nIndice> est entier]])
      //
      // Paramètres :
      //	bVisible (booléen - valeur par défaut=1) :Vrai pour afficher les boutons, Faux autrement
      //	nIndice (entier) : Indice des boutons à afficher / cacher
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE AfficheBoutonsAffichageZR(LOCAL bVisible est un booleen = vrai, LOCAL nIndice est un entier = gnIndiceSurvolzr)
      
      // L'indice doit être valide
      SI nIndice <= 0 _ou_ nIndice > ZR_Billets..Occurrence ALORS retour
      
      // Change la visibilité du bouton "Favoris"
      ATT_Favoris_Visible[nIndice] = bVisible
      // Change la bulle du bouton "Favoris"
      ATT_Favoris_Bulle[nIndice] = (ATT_Favoris[nIndice] ? <§$0066§> SINON <§$0067§>)
      
      // Change la visibilité du bouton "Archiver"
      // Ce bouton ne doit pas être visible si le billet ne peut pas être archivé
      bArchivable est un booleen
      sRaison est une chaine
      (bArchivable, sRaison) = Archivage_BilletEstArchivable(ATT_IDBillet[nIndice])
      ATT_Archiver_Visible[nIndice] = (bVisible ? bArchivable SINON faux)
      
      si optionlit(OPTION_IHM_TAGACTIF) = vrai alors ATT_Tags_Visible[nIndice] = bVisible sinon ATT_Tags_Visible[nIndice] = faux
      si optionlit(OPTION_IHM_partageaCTIF) = vrai alors ATT_Partage_Visible[nIndice] = bVisible sinon ATT_Partage_Visible[nIndice] = faux
      
     type : 458752
   -
     name : AfficheBilletsSélection
     internal_properties : 
     procedure_id : 1594494823351164384
     type_code : 14
     code : |1+
      // Résumé : Affiche les billets de la sélection courante de l'arbre (et met à jour des bulles d'options)
      // Syntaxe :
      //AfficheBilletsSélection ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE AfficheBilletsSélection()
      
      FEN_Principale..AffichageActif = Faux
      
      // Vide le tableau
      SupprimeTout(gtabBillets)
      
      // Par défaut, aucun paramètre
      REQ_ListeBillets.pFavoris = Null
      REQ_ListeBillets.pIDDossier = Null
      REQ_ListeBillets.pIDFluxRSS = Null
      REQ_ListeBillets.pIDTag = Null
      REQ_ListeBillets.pJour = Null
      REQ_ListeBillets.pLu = Null
      REQ_ListeBillets.pListeIDBillet = Null
      
      // Définit le libellé du bouton "MAJ courant" par défaut
      BTN_MAJ_Courant..Bulle = ChaîneConstruit(<§$000e§>, gPoliceGras(Vrai)+<§$000f§>+gPoliceGras(Faux)) +RC+ <§$0010§>
      // Définit le libellé de "Marquer lu courant" par défaut
      BTN_MarquerLu_Courant..Bulle = ChaîneConstruit(<§$0011§>, gPoliceGras(Vrai)+<§$000f§>+gPoliceGras(Faux)) +RC+ <§$0012§>
      
      // Selon la position du clic, le menu à ouvrir diffère
      SELON COL_ID[TABLEH_Flux]
      	// C'est un dossier
      	CAS [= PREFIX_DOSSIER
      		// Affiche tous les billets des flux du dossier
      		REQ_ListeBillets.pIDDossier = ExtraitChaîne(TABLEH_Flux.COL_ID[TABLEH_Flux], 2,"_")
      		// Définit le texte si le contenu est vide
      		LIB_AucunContenu = ChaîneConstruit(<§$0013§>,TABLEH_Flux.COL_LibelléOriginal[TABLEH_Flux])
      		// Définit le libellé du bouton "MAJ courant"
      		BTN_MAJ_Courant..Bulle = ChaîneConstruit(<§$0014§>, gPoliceGras(Vrai)+TABLEH_Flux.COL_LibelléOriginal[TABLEH_Flux]+gPoliceGras(Faux)) +RC+ <§$0010§>
      		// Définit le libellé de "Marquer lu courant"
      		BTN_MarquerLu_Courant..Bulle = ChaîneConstruit(<§$0015§>, gPoliceGras(Vrai)+TABLEH_Flux.COL_LibelléOriginal[TABLEH_Flux]+gPoliceGras(Faux)) +RC+ <§$0012§>
      		
      	// C'est la racine
      	CAS IDABONNEMENT
      		// Définit le texte si le contenu est vide
      		LIB_AucunContenu = <§$0016§>
      		// Définit le libellé du bouton "MAJ courant"
      		BTN_MAJ_Courant..Bulle = <§$0017§>+gPoliceGras(Vrai)+<§$000f§>+gPoliceGras(Faux) +RC+ <§$0010§>
      		// Définit le libellé de "Marquer lu courant"
      		BTN_MarquerLu_Courant..Bulle = <§$0018§>+gPoliceGras(Vrai)+<§$000f§>+gPoliceGras(Faux) +RC+ <§$0012§>
      		
      	// Dossier "veille"
      	CAS IDVEILLE
      		// Définit le texte de veille
      		LIB_AucunContenu = <§$0064§>
      		// Affiche le plan 3
      		FEN_Principale..Plan = 3
      		retour
      		
      	// Dossier "non lus"
      	CAS IDNONLUS
      		REQ_ListeBillets.pLu = Faux
      		// Définit le texte si le contenu est vide
      		LIB_AucunContenu = <§$0019§>
      		
      	// Dossier "favoris"
      	CAS IDFAVORIS
      		REQ_ListeBillets.pFavoris = Vrai
      		// Définit le texte si le contenu est vide
      		LIB_AucunContenu = <§$001a§>
      		// Définit le libellé de "Marquer lu courant"
      		BTN_MarquerLu_Courant..Bulle = ChaîneConstruit(<§$001b§>, gPoliceGras(Vrai)+<§$001c§>+gPoliceGras(Faux)) +RC+ <§$0012§>
      		
      	// Dossier "Aujourd'hui"
      	CAS IDAUJOURDHUI
      		REQ_ListeBillets.pJour = DateDuJour()
      		// Définit le texte si le contenu est vide
      		LIB_AucunContenu = <§$001d§>
      		// Définit le libellé de "Marquer lu courant"
      		BTN_MarquerLu_Courant..Bulle = ChaîneConstruit(<§$001e§>, gPoliceGras(Vrai)+<§$001f§>+gPoliceGras(Faux)) +RC+ <§$0012§>
      		
      	// Dossier "Hier"
      	CAS IDHIER
      		REQ_ListeBillets.pJour = Hier()
      		// Définit le texte si le contenu est vide
      		LIB_AucunContenu = <§$0020§>
      		// Définit le libellé de "Marquer lu courant"
      		BTN_MarquerLu_Courant..Bulle = ChaîneConstruit(<§$001e§>, gPoliceGras(Vrai)+<§$0021§>+gPoliceGras(Faux)) +RC+ <§$0012§>
      		
      	// Tags		
      	CAS [= PREFIX_TAG
      		// Affiche tous les billets marqués par le tag
      		REQ_ListeBillets.pIDTag = ExtraitChaîne(TABLEH_Flux.COL_ID[TABLEH_Flux], 2,"_")
      		// Définit le texte si le contenu est vide
      		LIB_AucunContenu = ChaîneConstruit(<§$0022§>, TABLEH_Flux.COL_LibelléOriginal[TABLEH_Flux])
      		// Définit le libellé de "Marquer lu courant"
      		BTN_MarquerLu_Courant..Bulle = ChaîneConstruit(<§$0023§>, gPoliceGras(Vrai)+TABLEH_Flux.COL_LibelléOriginal[TABLEH_Flux]+gPoliceGras(Faux)) +RC+ <§$0012§>
      		
      	// Dossier "tags"
      	CAS IDTAGS
      		// Définit le texte de veille
      		LIB_AucunContenu = <§$0065§>
      		// Affiche le plan 3
      		FEN_Principale..Plan = 3
      		RETOUR
      		
      	// C'est un flux
      	AUTRE CAS
      		// Affiche tous les billets du flux
      		REQ_ListeBillets.pIDFluxRSS = TABLEH_Flux.COL_ID[TABLEH_Flux]
      		// Définit le texte si le contenu est vide
      		LIB_AucunContenu = ChaîneConstruit(<§$0024§>, TABLEH_Flux.COL_LibelléOriginal[TABLEH_Flux])
      		// Définit le libellé du bouton "MAJ courant"
      		BTN_MAJ_Courant..Bulle = ChaîneConstruit(<§$0025§>, gPoliceGras(Vrai)+TABLEH_Flux.COL_LibelléOriginal[TABLEH_Flux]+gPoliceGras(Faux)) +RC+ <§$0010§>
      		// Définit le libellé de "Marquer lu courant"
      		BTN_MarquerLu_Courant..Bulle = ChaîneConstruit(<§$0026§>, gPoliceGras(Vrai)+TABLEH_Flux.COL_LibelléOriginal[TABLEH_Flux]+gPoliceGras(Faux)) +RC+ <§$0012§>
      FIN
      
      // Exécute la requête
      HExécuteRequête(REQ_ListeBillets)
      
      // Charge les résultats dans le tableau
      FichierVersTableau(gtabBillets, REQ_ListeBillets)
      
      // Mémorise les sélections (pour la table, la mémorisation est faite manuellement car on ne veut pas restituer la multisélection)
      nIndiceSélection est un entier = tableselect(TABLE_Billets, 1)
      nIndicePosition est un entier = tableposition(table_billets)
      sSauvePositionEtSélection est une chaine = ZoneRépétéeSauvePositionEtSélection(ZR_Billets)
      
      // Rafraîchit la table
      TableAffiche(TABLE_Billets, taInit)
      // Rafraîchit la ZR
      ZoneRépétéeAffiche(ZR_Billets, taInit)
      
      // Vide l'aperçu
      HTM_Apercu = ""
      
      // Repositionne les sélections
      nIndiceSélection = min(nIndiceSélection, table_billets..occurrence)
      nIndicePosition = min(nIndicePosition, table_billets..occurrence)
      si nIndiceSélection > 0 alors
      	// Se repositionne au bon endroit
      	tableposition(TABLE_Billets, nIndicePosition)
      	// Sélectionne la ligne
      	tableselectplus(TABLE_Billets, nIndiceSélection)
      	// Provoque la sélection
      	ExécuteTraitement(TABLE_Billets, trtSélection)
      Fin
      si sSauvePositionEtSélection <> "" alors ZoneRépétéerestaurePositionEtSélection(ZR_Billets)
      
      // Si aucun billet n'est listé
      SI gtabBillets..Occurrence = 0 ALORS
      	// Affiche le plan 3
      	FEN_Principale..Plan = 3
      SINON
      	// Repositionne le plan si besoin
      	SI FEN_Principale..Plan = 3 ALORS FEN_Principale..Plan = OptionLit(OPTION_IHM_MODE)
      FIN
      
      FIN:
      FEN_Principale..AffichageActif = Vrai
     type : 458752
   -
     name : ComptageApplique
     internal_properties : CAAAAAgAAADYU8QiNKSbwfaGO2WqGgs+g3Dk/NIP+e707kDFBWDS35LHwQwJVgTlfCRWiDaiFerIcDRltsKBQySwwb78qOyRg7RqgsSeExJqYv72AAk6IcIfBeG5QvcUkP2YEjfQQmtnrk3fAIVJgrQnYsmwdPWcqZ8r7tSpjDMsoaBKib1D8lqEtm5bSol0GH2vDYAAWWy64nUIG7A=
     procedure_id : 1596699507390472576
     type_code : 14
     code : |1-
      // Résumé : Applique le comptage dans l'arbre
      // Syntaxe :
      //ComptageApplique ( [<nIDFlux> est entier sur 8 octets [, <bMAJSélection> est booléen]])
      //
      // Paramètres :
      //	nIDFlux (entier sur 8 octets - valeur par défaut=0) :Identifiant du flux à mettre à jour
      //	bMAJSélection (booléen - valeur par défaut=1) :Vrai s'il s'agit d'ajout de billet, Faux s'il s'agit de lecture de billets
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE ComptageApplique(LOCAL nIDFlux est un entier sur 8 = 0, LOCAL bMAJSélection est un booleen = vrai)
      
      // S'il faut mettre uniquement le compteur du flux (et ses "parents") à jour
      si nIDFlux > 0 ALORS
      	
      	nIDDossier est un entier sur 8
      	
      	// Récupère l'identifiant du dossier
      	nPosition est un entier = hsauveposition(fluxrss, hSauveRubriques)
      	HLitRecherchePremier(fluxrss, IDFluxRSS, nIDFlux)
      	si htrouve(fluxrss) alors nIDDossier = fluxrss.IDDossier
      	HRetourPosition(nPosition)
      	
      	// Applique le comptage pour :
      	// - le flux
      	ComptageApplique_UnIdentifiant(nIDFlux, bMAJSélection)
      	// - son dossier
      	si FluxRSS.IDDossier > 0 alors ComptageApplique_UnIdentifiant(PREFIX_DOSSIER+nIDDossier, bMAJSélection)
      	// - la racine
      	ComptageApplique_UnIdentifiant(IDABONNEMENT, bMAJSélection)
      	// - les veilles
      	ComptageApplique_UnIdentifiant(IDNONLUS, bMAJSélection)
      	ComptageApplique_UnIdentifiant(IDFAVORIS, bMAJSélection)
      	ComptageApplique_UnIdentifiant(IDAUJOURDHUI, bMAJSélection)
      	ComptageApplique_UnIdentifiant(IDHIER, bMAJSélection)
      	
      SINON // S'il faut mettre tous les compteurs à jour
      	
      	// Parcourt le tableau de comptage
      	pour tout element nNbBillets, sIdentifiant de COL_Globales.gtaComptage
      		// Applique le comptage pour chaque identifiant
      		ComptageApplique_UnIdentifiant(sIdentifiant, bMAJSélection)	
      	FIN
      	
      FIN
      
      // Adapte l'icône du systray
      Systray_ChangePicto()
     type : 458752
   -
     name : MAJLance
     internal_properties : CAAAAAgAAADWu/0IAC3ubb9mljFIZw35peRova9zoSfW3czDM1M/oiYMtyR4B8WeREqH6ZvlLDmNYzExTgUE5b5mFySs1SsjLN/JcNwbAvh0iKfOoa2cXPHCPqSLnzOjq6xUNCyS4A5gS3Ju5MBv0TcOKveTZmPzhP6wNXhCHDOMXb5kPnpnBmPP63n3surBhohuztUKavKrxHmSTl+vTg==
     procedure_id : 1599025197737383472
     type_code : 14
     code : |1+
      // Résumé : Lance la mise à jour des flux demandés
      // Syntaxe :
      //MAJLance (<tabIDFlux> est tableau [, <bMAJAutomatique> est booléen])
      //
      // Paramètres :
      //	tabIDFlux (tableau) :Tableau contenant les identifiants de flux
      // 	bMAJAutomatique (booléen - valeur par défaut=1) : Vrai si la MAJ est lancé en mode automatique, Faux autrement
      // Valeur de retour :
      // 	Aucune
      //// Procédure automatique :
      // La procédure est exécutée manuellement, lors d'un appel dans le code
      // Elle s'exécutera dans un thread (sans avoir besoin d'appeler la fonction ThreadExecute), sans utilisation de HFSQL
      // 
      
      PROCEDURE MAJLance(tabIDFlux est un tableau d'entiers sur 8, LOCAL bMAJAutomatique est un booleen = vrai)
      
      tabTaches est un tableau de TâcheParallèle
      
      // Une mise à jour est en cours
      ExécuteThreadPrincipal(MAJEnCours, vrai)
      
      // Parcourt les éléments
      pour tout element nIDFlux de tabIDFlux
      	
      	// Lance une tâche parallèle pour récupérer les billets du flux
      	MaTache est une TâcheParallèle = TâcheParallèleExécute(TP_rssRécupèreBilletsFlux, (nIDFlux))
      	
      	// Mémorise la tâche
      	ajoute(tabTaches, MaTache)
      	
      FIN
      
      // Attend la fin d'exécution de toutes les tâches
      TâcheParallèleAttendToutes(tabTaches)
      
      //  Il n'y a plus de MAJ en cours
      ExécuteThreadPrincipal(MAJEnCours, faux)
      
      // Envoie le message de fin si besoin (en mode automatique uniquement)
      si bMAJAutomatique alors MAJAutomatique_Message()
      
      // Mémorise la date et heure de dernière MAJ
      gdhMAJDernière = Maintenant()
     type : 458752
   -
     name : ComptageApplique_UnIdentifiant
     procedure_id : 1599053398518285890
     type_code : 14
     code : |1-
      // Résumé : Applique le comptage pour un élément donné
      // Syntaxe :
      //[ <Résultat> = ] ComptageApplique_UnIdentifiant (<sIdentifiant> est chaîne [, <bMAJSélection> est booléen])
      //
      // Paramètres :
      //	sIdentifiant (chaîne ANSI) :Identifiant de l'élément
      //	bMAJSélection (booléen - valeur par défaut=1) :Vrai s'il s'agit d'ajout de billet, Faux s'il s'agit de lecture de billets
      // Valeur de retour :
      // 	booléen : Vrai si l'ajout est effectif, Faux autrement
      //
      PROCEDURE PRIVÉE ComptageApplique_UnIdentifiant(LOCAL sIdentifiant est une chaine, LOCAL bMAJSélection est un booleen = vrai) : booleen
      
      nIndice est un entier
      nNbBillets est un entier
      
      // Le comptage doit être disponible
      si COL_Globales.gtaComptage[sIdentifiant]..vide alors RENVOYER Faux
      
      // Mémorise le nombre de billets
      nNbBillets = COL_Globales.gtaComptage[sIdentifiant]
      
      // Cherche la ligne
      nIndice = TableCherche(TABLEH_Flux.COL_ID, sIdentifiant)
      SI nIndice <= 0 ALORS renvoyer faux
      
      // Si la ligne en cours de traitement est la ligne actuellement sélectionnée (et si on a demandé à mettre à jour l'affichage)
      SI bMAJSélection _et_ nIndice = TableSelect(TABLEH_Flux) ALORS
      	// Met à jour l'affichage
      	AfficheBilletsSélection_MAJ()
      FIN
      
      // Modifie la colonne de comptage
      TABLEH_Flux.COL_Compteur[nIndice] = Max(0,nNbBillets)
      // Modifie l'affichage
      SI TABLEH_Flux.COL_Compteur[nIndice] = 0 ALORS
      	// Repositionne le libellé original
      	TABLEH_Flux.COL_Libellé[nIndice] = TABLEH_Flux.COL_LibelléOriginal[nIndice]
      	// Supprime le gras de la ligne
      	TABLEH_Flux.COL_Libellé[nIndice]..PoliceGras = Faux
      SINON
      	// Affiche le libellé original avec le compteur à côté
      	TABLEH_Flux.COL_Libellé[nIndice] = TABLEH_Flux.COL_LibelléOriginal[nIndice] + " ("+nNbBillets+")"
      	// Passe la ligne en gras (sauf pour les favoris et tags, le comptage ne correspondant pas à un "non lu")
      	SI PAS (sIdentifiant = IDFAVORIS _OU_ sIdentifiant [= PREFIX_TAG) ALORS
      		TABLEH_Flux.COL_Libellé[nIndice]..PoliceGras = Vrai
      	fin
      FIN
      
      renvoyer vrai
     type : 458752
