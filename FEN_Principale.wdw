     name : DétectePremierLancement
     procedure_id : 1590855796402274604
     type_code : 14
     code : |1+
      // Résumé : Vérifie s'il s'agit du premier lancement de l'application et propose des actions adaptées
      // Syntaxe :
      // DétectePremierLancement ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE DétectePremierLancement()
      
      // Si la base ne contient aucun flux
      SI HNbEnr(FluxRSS) = 0 ALORS
      	
      	// Ouvre la fenêtre de premier lancement
      	sFenêtreImport est une chaine = Ouvre(FEN_PremierLancement)
      	
      	// Si l'option choisie nécessite l'ouverture d'une fenêtre (import d'un fichier OPML par exemple)
      	selon sFenêtreImport
      		// Aucun retour
      		CAS ""
      			// Ne rien faire
      		
      		// default (flux par défaut importés)
      		CAS "default"
      			// Met à jour l'affichage
      			RemplitArbre()
      		
      		// autres cas : fenêtre d'import ou d'ajout de flux
      		CAS "FEN_Import"
      		
      		// Ouvre la fenêtre demandée
      		tabIDFlux est un tableau d'entiers sur 8 = Ouvre(FEN_Import)
      		si tabIDFlux..Occurrence > 0 ALORS
      			// Met à jour l'affichage
      			RemplitArbre()			
      		FIN
      		
      		CAS "FEN_NouveauFlux"
      			// Ouvre la fenêtre demandée
      			nIDFlux est un entiers sur 8 = Ouvre(FEN_NouveauFlux)
      			SI nIDFlux > 0 ALORS
      				// Met à jour l'affichage
      				RemplitArbre()				
      
      			FIN
      			
      	FIN
      	
      FIN
