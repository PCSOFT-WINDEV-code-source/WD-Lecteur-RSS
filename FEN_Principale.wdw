         	FIN
         FIN
         
         // Flux lu ?
         ATT_Titre_Couleur = (ATT_Lu ? GrisFoncé SINON Noir)
         ATT_Image_Opacité = (ATT_Lu ? 20 SINON 100)
         
         // Image du flux
         SI ATT_ImageFlux ~= "" ALORS ATT_ImageFlux = FluxRSS_RécupèreImageCache(att_idflux)
         
         // Boutons :
         // -  Favoris
         ATT_FavorisImage = (ATT_Favoris ? IMG_FavorisPlein SINON IMG_Favoris)
         ATT_Titre_Couleur = (ATT_Favoris ? HTMLVersRVB("#E91E63") SINON ATT_Titre_Couleur)
         // - Tags
         // Tags associés
         ATT_TagsAssociés = Billet_ListeTagsAssociés(ATT_IDBillet)
         ATT_TagImage = (ATT_TagsAssociés ~= "" ? IMG_TAG SINON IMG_TAG_Bleu)
         // Tags
         sBulle est une chaine = <§$0046§>
         // Affiche les tags déjà affectés
         SI ATT_TagsAssociés <> "" ALORS
         	sBulle += RC+RC+ gPoliceItalique(Vrai)+gStylo(HTMLVersRVB("#2196F3")) + Remplace(ATT_TagsAssociés, ";", RC)
         FIN
         ATT_TagBulle = sBulle
