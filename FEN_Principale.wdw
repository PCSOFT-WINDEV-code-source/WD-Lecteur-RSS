   -
     name : Billet_OuvreDansNavigateur
     procedure_id : 1593059913367273123
     type_code : 14
     code : |1-
      // Résumé : Affiche le billet
      // Syntaxe :
      //Billet_OuvreDansNavigateur (<nIDBillet> est entier sur 8 octets)
      //
      // Paramètres :
      //	nIDBillet (entier sur 8 octets) :Identifiant du billet
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE Billet_OuvreDansNavigateur(LOCAL nIDBillet est un entier sur 8)
      
      nPositionBillet est un entier
      nPositionFlux 	est un entier
      sLien 			est une chaine
      
      nPositionBillet = hsauveposition(billet, hsauverubriques)
      nPositionFlux = hsauveposition(Fluxrss, hsauverubriques)
      
      // Recherche le billet
      hlitrecherchepremier(billet, IDBillet, nIDBillet)
      si htrouve(billet) ALORS
      	
      	// Lance le navigateur par défaut sur le billet
      	si billet.lien <> "" alors
      		sLien = billet.lien
      	SINON
      		HLitRecherchePremier(FluxRSS, IDFluxRSS, billet.IDFluxRSS)
      		si htrouve(FluxRSS) ALORS
      			// Un lien est-il défini sur le flux ?
      			si fluxrss.Lien <> "" alors
      				sLien = Billet.Lien
      			SINON
      				// En dernier recours, mémorise le lien du flux
      				sLien = fluxrss.Adresse
      			FIN
      		FIN
      	Fin
      FIN
      
      // Si un lien a été trouvé
      si sLien <> "" alors
      	lanceappliassociée(Billet.Lien)
      SINOn
      	// S'il n'y a pas de lien, il faut avertir l'utilisateur
      	ToastSupprimeTout()
      	ToastAffiche(<§$0005§> +RC+ <§$0006§>, toastCourt, cvMilieu, chCentre)
      Fin
      
      
      FIn :
      HRetourPosition(nPositionBillet)
      HRetourPosition(nPositionFlux)
     type : 458752
   -
     name : RemplitArbre_Dossier
     procedure_id : 1593437553454769950
     type_code : 14
     code : |1-
      // Résumé : Ajoute une branche "dossier"
      // Syntaxe :
      //[ <Résultat> = ] RemplitArbre_Dossier (<nIDDossier> est entier sur 8 octets)
      //
      // Paramètres :
      //	nIDDossier (entier sur 8 octets) :Identifiant du dossier
      // Valeur de retour :
      // 	entier : Indice du dossier
      //
      PROCEDURE RemplitArbre_Dossier(LOCAL nIDDossier est un entier sur 8) : entier
      
      nIndiceDossier est un entier = -1
      nPosition est un entier = -1
      
      
      nPosition = hsauveposition(dossier, hsauverubriques)
      
      // Lit le dossier
      HLitRecherchePremier(Dossier, IDDossier, nIDDossier)
      SI HTrouve(Dossier) = Faux ALORS RENVOYER -1
      
      // Recherche l'indice du dossier
      nIndiceDossier = TableCherche(TABLEH_Flux.COL_ID, PREFIX_DOSSIER+nIDDossier)
      
      // Si le dossier n'est pas encore présent (il vient d'être ajouté par exemple)
      SI nIndiceDossier <= 0 ALORS
      	
      	// Définit les images pour la nouvelle branche	
      	TABLEH_Flux..ImageDéroulée = IMG_Répertoire_Ouvert
      	TABLEH_Flux..ImageEnroulée = IMG_Répertoire_Fermé
      	
      	// Le dossier est forcément lié au dossier "abonnements"
      	nIndiceParent est un entier = TableCherche(TABLEH_Flux.COL_ID, IDABONNEMENT)
      	si nIndiceParent <= 0 alors renvoyer -1
      	
      	// Ajoute le dossier à la racine
      	nIndiceDossier = TableAjouteFils(TABLEH_Flux, nIndiceParent, PREFIX_DOSSIER+nIDDossier, Dossier.Libellé, 0, "0"+dossier.libelle, dossier.libelle)
      	
      FIN
      
      // Met à jour les éléments du flux qui peuvent changer
      tableh_flux[nIndiceDossier].COL_LibelléOriginal = dossier.libelle
      tableh_flux[nIndiceDossier].COL_Tri 			= "0"+dossier.libelle
      
      // Trie la branche "Abonnement
      TableTrieFils(TABLEH_Flux, nIndiceParent, "COL_Tri")
      
      // Re récupère l'indice (qui a pu être modifié par le tri)
      nIndiceDossier = TableCherche(TABLEH_Flux.COL_ID, PREFIX_DOSSIER+nIDDossier)
      
      // Met à jour le compteur du dossier
      //MAJ_Compteur_Dossier(nIDDossier)
      
      renvoyer nIndiceDossier
      
      fin : 
      si nPosition <> -1 alors hretourposition(nPosition)
     type : 458752
   -
     name : RemplitArbre_Flux
     procedure_id : 1593439456133655324
     type_code : 14
     code : |1-
      // Résumé : Ajoute un flux dans l'arbre
      // Syntaxe :
      //[ <Résultat> = ] RemplitArbre_Flux (<nIDFluxRSS> est entier sur 8 octets)
      //
      // Paramètres :
      //	nIDFluxRSS (entier sur 8 octets) :Identifiant du flux
      // Valeur de retour :
      // 	entier : Indice du flux
      //
      PROCEDURE RemplitArbre_Flux(nIDFluxRSS est un entier sur 8) : entier
      
      nIndiceFlux est un entier
      sImageFlux est une chaine
      nIndiceDossier est un entier
      nPosition est un entier = -1
      
      
      nPosition = hsauveposition(fluxrss, hsauverubriques)
      
      // Lit l'enregistrement
      HLitRecherchePremier(FluxRSS, IDFluxRSS, nIDFluxRSS)
      SI HTrouve(FluxRSS) = Faux ALORS RENVOYER -1
      
      // Cherche si le flux est déjà présent
      nIndiceFlux = tablecherche(TABLEH_Flux.COL_ID, nIDFluxRSS)
      si nIndiceFlux <= 0 alors
      	
      	// Récupère l'indice du dossier
      	si FluxRSS.IDDossier >  0 alors
      		nIndiceDossier = RemplitArbre_Dossier(fluxrss.IDDossier)
      	SINON
      		nIndiceDossier = tablecherche(TABLEH_Flux.COL_ID, IDABONNEMENT)
      	Fin
      	
      	// L'indice du dossier parent doit être valide
      	si nIndiceDossier <= 0 alors renvoyer -1
      	
      	// Ajoute le flux
      	nIndiceFlux = TableAjouteFils(TABLEH_Flux, nIndiceDossier, FluxRSS.IDFluxRSS, fluxrss.titre, 0)
      		
      Fin
      
      // Met à jour les éléments du flux qui peuvent changer
      TABLEH_Flux[nIndiceFlux].COL_LibelléOriginal = FluxRSS.Titre
      TABLEH_Flux[nIndiceFlux].COL_Tri = "1"+FluxRSS.Titre
      
      // Définit l'image
      sImageFlux = FluxRSS_RécupèreImageCache(nIDFluxRSS)	
      TABLEH_Flux[nIndiceFlux]..ImageDéroulée = sImageFlux
      TABLEH_Flux[nIndiceFlux]..ImageEnroulée = sImageFlux
      
      // Trie le dossier parent
      TableTrieFils(TABLEH_Flux, nIndiceDossier, "COL_Tri")
      
      // Récupère l'indice du flux (qui a pu être modifié par le tri)
      nIndiceFlux = TableCherche(TABLEH_Flux.COL_ID, nIDFluxRSS)
      
      // Met à jour le compteur du flux
      //MAJ_Compteur_FluxRSS(nIDFluxRSS)
      
      // Renvoie l'indice du flux
      renvoyer nIndiceFlux
      
      Fin :
      si nPosition <> -1 alors HRetourPosition(nPosition)
     type : 458752
