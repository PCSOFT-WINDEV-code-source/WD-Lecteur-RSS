   -
     name : ChangeModeAffichage
     procedure_id : 1599313750827416496
     type_code : 14
     code : |1+
      // Résumé : Change le mode d'affichage
      // Syntaxe :
      //ChangeModeAffichage (<nNouveauMode> est entier)
      //
      // Paramètres :
      //	nNouveauMode (entier) :Mode d'affichage à utiliser (correspond au plan)
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE ChangeModeAffichage(LOCAL nNouveauMode est un entier)
      
      // Force l'état enfoncé du nouveau mode à Vrai
      {"BTN_Affichage_"+nNouveauMode, indChamp}..Enfoncé = vrai
      
      // Récupère le mode d'affichage courant
      nModeCourant est un entier = optionlit(OPTION_IHM_MODE)
      
      // Si le mode d'affichage courant est celui demandé, ne rien faire
      si nNouveauMode = nModeCourant alors retour
      
      // Modifie l'état enfoncé du bouton "mode d'affichage" courant
      {"BTN_Affichage_"+nModeCourant, indchamp}..Enfoncé = Faux
      
      // Mémorise le mode d'affichage
      OptionEcrit(OPTION_IHM_MODE, nNouveauMode)
      
      // Si l'application est sur le plan "aucun contenu", il ne faut pas changer
      SI MaFenêtre..Plan = 3 ALORS RETOUR
      
      // Change l'affichage avec un effet
      AnimationPrépare(MaFenêtre)
      MaFenêtre..Plan = nNouveauMode
      AnimationJoue(animFondu)
