      // Syntaxe :
      //Tags_PersonnaliseMenuPourBillet (<nIDBillet> est entier sur 8 octets)
      //
      // Paramètres :
      //	nIDBillet (entier sur 8 octets) : Identifiant du billet
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE Tags_PersonnaliseMenuPourBillet(LOCAL nIDBillet est un entier sur 8)
      
      tabCatégories est un tableau de chaines
      nIndiceTagExistant est un entier
      
      
      // Recherche le billet
      hlitrecherchepremier(billet, idbillet, nIDBillet)
      si htrouve(billet) = faux alors retour
      
      // Récupère les catégories du billet
      chaineverstableau(billet.Categorie, tabCatégories,";")
      tableautrie(tabCatégories, ttCroissant)
      
      // Il faut supprimer les suggestions précédentes
      nIndice est un entier = 1
      sOption est une chaine = OPTIONTAGSSUGG+nIndice
      tantque Menuexiste(MENU_Billet_Tag, sOption)
      	// Supprime l'option
      	menusupprime(sOption)
      	// Option suivante
      	nIndice++
      	sOption = OPTIONTAGSSUGG+(nIndice)
      FIN
      
      
      // Pour chaque tag
      nIndiceTagExistant = 1
      pour TOUT Tag sur libellé
      
      	// Mémorise l'indice du tag dans les suggestions
      	nIndice = cherche(tabCatégories, tcLinéaire, tag.Libellé)
      	
      	// Définit le nom de l'option
      	sOption =  OPTIONTAGS+nIndiceTagExistant
      	
      	// Si une association existe pour ce tag
      	hrecherchepremier(association, IDAssociation,[nIDBillet, tag.idtag])
      	si htrouve(association) ALORS 
      		// Coche le tag et le passe en gras
      		{sOption, indChamp}..image = IMG_CochePleine
      		{sOption, indChamp}..policegras = Vrai
      		
      	SINON
      		// Tag non associé
      		{sOption, indChamp}..Image = IMG_Cochevide
      		{sOption, indChamp}..PoliceGras = faux
      		
      		// Si ce tag est une proposition, il faut la mettre en avant (en gras et bleu)
      		SI nIndice > 0 ALORS
      			{sOption, indChamp}..Couleur = htmlversrvb("#2196F3")
      		FIN
      		
      	FIN
      	
      	// Si ce tag est une proposition, il faut l'enlever de la liste des propositions
      	SI nIndice > 0 ALORS Supprime(tabCatégories, nIndice)
      	
      	// Indice suivant
      	nIndiceTagExistant++
      
      FIn
      
      
      nIndiceTagExistant = 1
      // Parcourt les suggestions (triées par ordre alphabétique)
      pour tout element sUneSuggestion, nIndice de tabCatégories
      	
      	// La suggestion ne doit pas être vide
      	si sUneSuggestion dans ("", eot) alors continuer
      	
      	// Avance parmi les options existantes
      	tantque menuexiste(MENU_Billet_Tag, OPTIONTAGS+nIndiceTagExistant) _et_ minuscule({ OPTIONTAGS+nIndiceTagExistant, indChamp}..libellé) < minuscule(sUneSuggestion)
      		nIndiceTagExistant++
      	FIN
      
      	// Définit le nom de l'option
      	sOption = OPTIONTAGSSUGG+nIndice
      	
      	si MenuExiste(MENU_Billet_Tag,  OPTIONTAGS+nIndiceTagExistant) ALORS
      		// Insère l'option
      		MenuInsèreOption(MENU_Billet_Tag,  OPTIONTAGS+nIndiceTagExistant, sOption, sUneSuggestion, Tags_Ajoute, sUneSuggestion)
      	SINON
      		// Ajoute l'option
      		MenuajouteOption(MENU_Billet_Tag, sOption, sUneSuggestion, Tags_Ajoute, sUneSuggestion)
      	FIN
      	// Coche vide
      	{sOption, indChamp}..Image = IMG_CocheVide
      	// Suggestion, donc en bleu
      	{sOption, indChamp}..Couleur = HTMLVersRVB("#2196F3")
      	
      FIN
     type : 458752
