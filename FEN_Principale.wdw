     code : |1+
      // Résumé : Change le picto du systray selon le nombre de message non lu
      // Syntaxe :
      //Systray_ChangePicto ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE Systray_ChangePicto()
      
      nNbBilletsNonLus 	est un entier
      sIcone 				est une chaine
      sMessage 			est une chaine
      
      // Paramètre la requête (il faut compter tous les billets non lus)
      REQ_NbBilletsNonLus.pDébut 		= null
      REQ_NbBilletsNonLus.pfin 		= null
      REQ_NbBilletsNonLus.pIDDossier 	= null
      REQ_NbBilletsNonLus.pIDFlux 	= null
      
      // Exécute la requête
      si hexécuterequete(REQ_NbBilletsNonLus) = faux alors retour
      
      // Lit le résultat
      hlitpremier(REQ_NbBilletsNonLus)
      si hendehors(REQ_NbBilletsNonLus) = faux ALORS
      	// Mémorise le nombre de billets non lus
      	nNbBilletsNonLus = REQ_NbBilletsNonLus.NbBillets
      FIN
      
      // Selon le nombre de billet non lu, défini l'icone et le message
      selon nNbBilletsNonLus
      	// Tous les billets sont lus
      	CAS 0
      		sIcone = ".\rss.ico"
      		sMessage = <§$0002§> +RC+ <§$0029§>
      	
      	// 1 billet est non lu
      	CAS 1
      		sIcone = ".\rss-new.ico"
      		sMessage = <§$0002§> +RC+ <§$002a§>
      	
      	// Au moins 2 billets sont non lus
      	Autres cas
      		sIcone = ".\rss-new.ico"
      		sMessage = <§$0002§> +RC+ nNbBilletsNonLus+<§$002b§>
      FIN
      
      
      // Modifie l'icône
      SysIconeModifie(sIcone, sMessage)
