     code : |1-
      // Résumé : Procédure appelée lors du survol en mode DnD
      // Syntaxe :
      //DnD_Survol ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE DnD_Survol()
      
      // Récupère l'indice de la ligne
      nIndiceLigne est un entier = tableinfoxy(TABLEH_Flux, tiNumLigne, sourisposx(), sourisposy())
      // L'indice doit être valide
      si nIndiceLigne <= 0 alors
      	DnDCurseur(dndInterdit)
      	retour
      FIN
      
      // Le dnd doit être actif (sur un flux
      si gnIDFluxDéplacé <= 0 alors
      	DnDCurseur(dndInterdit)
      	retour
      FIN
      
      // Selon l'identifiant correspondant, autorise ou non le DnD
      selon TABLEH_Flux.COL_ID[nIndiceLigne]
      
      	// Dossier "veille" ou tags : dnd non autorisé
      	CAS IDVEILLE, IDNONLUS, IDFAVORIS, IDAUJOURDHUI, IDHIER, IDTAGS, [= PREFIX_TAG
      		DnDCurseur(dndInterdit)
      	
      	// Autres cas : dossier racine, dossier : dnd autorisé
      	AUTRE CAS
      		DnDCurseur(dndDéplacer)
      		
      FIN
     type : 458752
   -
     name : DnD_Lâcher
     procedure_id : 1601607748801411366
     type_code : 14
     code : |1-
      // Résumé : Procédure appelée lors du lâcher en mode DnD
      // Syntaxe :
      //DnD_Lâcher ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE DnD_Lâcher()
      
      // Récupère l'indice de la ligne
      nIndiceLigne est un entier = tableinfoxy(TABLEH_Flux, tiNumLigne, sourisposx(), sourisposy())
      // L'indice doit être valide
      si nIndiceLigne <= 0 alors
      	DnDCurseur(dndInterdit)
      	retour
      FIN
      
      nIDDossier est un entier sur 8
      // Selon l'identifiant correspondant du lâcher, il faut déplacer le flux au bon endroit
      selon TABLEH_Flux.COL_ID[nIndiceLigne]
      
      	// Il s'agit du dossier "Abonnements"
      	cas idabonnement
      		// Le flux a maintenant comme parent le dossier racine
      		nIDDossier = 0
      		
      	// Il s'agit d'un dossier
      	CAS [= PREFIX_DOSSIER
      		// Le flux a maintenant comme parent ce dossier
      		nIDDossier = extraitchaine(TABLEH_Flux.COL_ID[nIndiceLigne], 2, PREFIX_DOSSIER)
      		
      	// Il s'agit d'un flux
      	AUTRE CAS
      		// Le flux a maintenant le même parent que ce flux
      		nIndiceParent est un entier = TableRécupèreParent(TABLEH_Flux,nIndiceLigne)
      		// L'indice doit être valide
      		SI nIndiceParent <= 0 ALORS
      			DnDCurseur(dndInterdit)
      			RETOUR
      		FIN
      		
      		// Si le parent est le dossier racine
      		si TABLEH_Flux.COL_ID[nIndiceParent] = IDABONNEMENT ALORS
      			nIDDossier = 0
      		SINOn // Si le parent est un dossier
      			nIDDossier = ExtraitChaîne(TABLEH_Flux.COL_ID[nIndiceParent], 2, PREFIX_DOSSIER)
      		FIN
      		
      FIN
      
      // Modifie le flux
      HLitRecherchePremier(fluxrss, IDFluxRSS, gnIDFluxDéplacé)
      si htrouve(fluxrss) = faux alors
      	DnDCurseur(dndInterdit)
      	RETOUR
      FIN
      fluxrss.IDDossier = nIDDossier
      hmodifie(fluxrss)
      
      
      // Supprime la ligne courante (le flux a été déplacé)
      nIndiceOrigine est un entier = tablecherche(TABLEH_Flux.COL_ID, gnIDFluxDéplacé)
      // L'indice doit être valide
      SI nIndiceOrigine <= 0 ALORS
      	DnDCurseur(dndInterdit)
      	RETOUR
      FIN
      // Supprime la ligne d'origine
      TableSupprime(TABLEH_Flux, nIndiceOrigine)
      
      // Met à jour l'affichage du flux
      FluxRSS_Affiche(gnIDFluxDéplacé)
      // Applique le comptage pour chaque identifiant
      ComptageApplique_UnIdentifiant(gnIDFluxDéplacé, 0)	
      
      // Cherche le flux
      nIndice est un entier = tablecherche(tableh_flux.col_id, gnIDFluxDéplacé)
      SI nIndice <= 0 ALORS RETOUR
      // Sélectionne le flux
      TableSelectPlus(TABLEH_Flux, nIndice)
      // Met à jour l'IHM
      ExécuteTraitement(TABLEH_Flux, trtSélection)
     type : 458752
   -
     name : DnD_DébutGlisser
     procedure_id : 1601607912010272400
     type_code : 14
     code : |1+
      // Résumé : Procédure appelée lors du début de glisser en mode DnD
      // Syntaxe :
      // DnD_DébutGlisser ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE DnD_DébutGlisser()
      
      // Par défaut, aucun flux en déplacement
      gnIDFluxDéplacé = -1
      
      // Récupère l'indice de la ligne
      nIndiceLigne est un entier = tableinfoxy(TABLEH_Flux, tiNumLigne, sourisposx(), sourisposy())
      // L'indice doit être valide
      SI nIndiceLigne <= 0 ALORS
      	DnDCurseur(dndinterdit)
      	RETOUR
      FIN
      
      // La ligne doit correspondre à un flux
      si estnumerique(TABLEH_Flux.COL_id[nIndiceLigne]) = faux ALORS
      	DnDCurseur(dndInterdit)
      	RETOUR
      FIN
      
      // Autorise le déplacement
      DnDCurseur(dndDéplacer)
      
      // Mémorise l'identifiant du flux
      gnIDFluxDéplacé = TABLEH_Flux.COL_ID[nIndiceLigne]
