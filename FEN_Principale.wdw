     name : MAJAutomatique_Message
     procedure_id : 1593456146409354831
     type_code : 14
     code : |1-
      // Résumé : Envoie un message en fin de MAJ automatique (si de nouveaux flux sont disponibles)
      // Syntaxe :
      //MAJAutomatique_Message ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE MAJAutomatique_Message()
      
      sMessage est une chaine
      
      
      // Si la fenêtre est invisible uniquement ou que l'utilisateur a demandé a toujours voir la bulle de nouveau contenu
      SI gbFenêtreVisible _ET_ Val(OptionLit(OPTION_ALERTE_SEULEMENTSIREDUIT)) ALORS RETOUR
      
      // Récupère tous les billets dont la date de réception est supérieure à la date de dernière MAJ
      REQ_ListeBillets_Reception.pDateRéception = gdhMAJDernière
      si HExécuteRequête(REQ_ListeBillets_Reception) = faux alors retour
      
      // Parcourt les résultats
      pour TOUT REQ_ListeBillets_Reception 
      	
      	// Ajoute une ligne
      	sMessage += [RC] + ChaîneConstruit(<§$000c§>, REQ_ListeBillets_Reception.Titre, REQ_ListeBillets_Reception.NbBillets)
      	
      FIN
      
      // Si un message doit être affiché (= s'il y a de nouveaux billets)
      si sMessage <> "" alors
      	
      	// Affiche une bulle indiquant un nouveau contenu
      	SysIconeAfficheBulle(sMessage, <§$000d§>, sysTypeBulleDéfaut, "", SysTray_BulleClic)
      	
      Fin
