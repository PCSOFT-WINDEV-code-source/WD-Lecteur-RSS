     name : AfficheBilletsSélection_MAJ
     procedure_id : 1599766763228933319
     type_code : 14
     code : |1+
      // Résumé : Affiche les billets de la sélection courante (en cas de MAJ)
      // Syntaxe :
      //AfficheBilletsSélection_MAJ ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE AfficheBilletsSélection_MAJ()
      
      FEN_Principale..AffichageActif = Faux
      
      tabBilletsAJour est un tableau de STBillet
      
      // Recharge les billets courants
      REQ_ListeBillets.pListeIDBillet = ""
      POUR TOUT element stUnBillet de gtabBillets
      	REQ_ListeBillets.pListeIDBillet += [";"] + stUnBillet.nIDBillet
      FIN
      // Exécute la requête
      HExécuteRequête(REQ_ListeBillets)
      // Charge les résultats dans le tableau
      FichierVersTableau(gtabBillets, REQ_ListeBillets)
      
      
      // Par défaut, aucun paramètre
      REQ_ListeBillets.pFavoris = Null
      REQ_ListeBillets.pIDDossier = Null
      REQ_ListeBillets.pIDFluxRSS = Null
      REQ_ListeBillets.pIDTag = Null
      REQ_ListeBillets.pJour = Null
      REQ_ListeBillets.pLu = Null
      REQ_ListeBillets.pListeIDBillet = Null
      
      // Selon la position du clic, le menu à ouvrir diffère
      SELON COL_ID[TABLEH_Flux]
      	// C'est un dossier
      	CAS [= PREFIX_DOSSIER
      		// Affiche tous les billets des flux du dossier
      		REQ_ListeBillets.pIDDossier = ExtraitChaîne(TABLEH_Flux.COL_ID[TABLEH_Flux], 2,"_")
      			
      	// C'est la racine
      	CAS IDABONNEMENT
      		
      	// Dossier "veille"
      	CAS IDVEILLE
      		
      	// Dossier "non lus"
      	CAS IDNONLUS
      		REQ_ListeBillets.pLu = Faux
      	
      	// Dossier "favoris"
      	CAS IDFAVORIS
      		REQ_ListeBillets.pFavoris = Vrai
      	
      	// Dossier "Aujourd'hui"
      	CAS IDAUJOURDHUI
      		REQ_ListeBillets.pJour = DateDuJour()
      	
      	// Dossier "Hier"
      	CAS IDHIER
      		REQ_ListeBillets.pJour = Hier()
      		
      	// Tags		
      	CAS [= PREFIX_TAG
      		// Affiche tous les billets marqués par le tag
      		REQ_ListeBillets.pIDTag = ExtraitChaîne(TABLEH_Flux.COL_ID[TABLEH_Flux], 2,"_")
      	
      	// Dossier "tags"
      	CAS IDTAGS
      
      	// C'est un flux
      	AUTRE CAS
      		// Affiche tous les billets du flux
      		REQ_ListeBillets.pIDFluxRSS = TABLEH_Flux.COL_ID[TABLEH_Flux]
      
      FIN
      
      // Exécute la requête
      HExécuteRequête(REQ_ListeBillets)
      
      // Charge les résultats dans un tableau
      FichierVersTableau(tabBilletsAJour, REQ_ListeBillets)
      
      // Ajoute les nouveaux billets
      pour tout element stUnNouveauBillet de tabBilletsAJour
      	si cherche(gtabBillets, tcLinéaire, "nIDBillet", stUnNouveauBillet.nIDBillet) <= 0 ALORS
      		ajoute(gtabbillets, stUnNouveauBillet)
      	FIN
      FIN
      
      // Tri les résultats par date, titre
      tableautrie(gtabbillets,ttMembre,"-dhDPublication;+sTitre")
      
      // Mémorise les sélections
      nIDBilletTable est un entier sur 8
      nIndice est un entier = TableSelect(TABLE_Billets)
      si nIndice > 0 alors nIDBilletTable = TABLE_Billets.COL_IDBillet[nIndice]
      
      // Rafraîchit la table
      TableAffiche(TABLE_Billets, taInit)
      // Rafraîchit la ZR
      ZoneRépétéeAffiche(ZR_Billets, taInit)
      
      // Vide l'aperçu
      HTM_Apercu = ""
      
      // Repositionne les sélections
      si nIDBilletTable > 0 alors
      	nIndice = tablecherche(TABLE_Billets.COL_IDBillet, nIDBilletTable)
      	si nIndice > 0 alors tableselectplus(TABLE_Billets, nIndice) ; executetraitement(TABLE_Billets, trtselection)
      FIN
      
      // Si aucun billet n'est listé
      SI gtabBillets..Occurrence = 0 ALORS
      	// Affiche le plan 3
      	FEN_Principale..Plan = 3
      SINON
      	// Repositionne le plan si besoin
      	SI FEN_Principale..Plan = 3 ALORS FEN_Principale..Plan = OptionLit(OPTION_IHM_MODE)
      FIN
      
      FIN:
      FEN_Principale..AffichageActif = Vrai
