     name : RemplitArbre
     internal_properties : CAAAAAgAAABH6+U4haIzoZYKMq9I/i+LdhdUEQQdmlMYSIkpo94Qg/eD+FRM7y+x+/Q23+rRyQdwjRatbwjEZQzI8WIxrBW4p5RhfTVgxoMlk6lcRuQuITekWAwUgTNVqMdOgeR4mveZjMJhr8FxRrugXtD5rpUgPVd+CvmzjEdNIJyvkDct1FvqYY+V8PLEr//dlPBLsSWHACkSYMFZfy/9xz1mgEHz3dkfSNtKq4mO5KbuUt7kElPqKh0=
     procedure_id : 1554848658748130637
     type_code : 14
     code : |1-
      // Résumé : Remplit l'arbre
      // Syntaxe :
      //RemplitArbre ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE RemplitArbre()
      
      nIndice 			est un entier
      nIndiceDossier	 	est un entier
      
      
      // Vide la liste
      tablesupprimetout(TABLEH_Flux)
      
      // Veille
      RemplitArbre_Veille()
      
      // Abonnements
      TABLEH_Flux..ImageDéroulée = IMG_RépertoireRSS
      TABLEH_Flux..Imageenroulée = IMG_RépertoireRSS
      nIndice = tableajouteligne(TABLEH_Flux, IDABONNEMENT, <§$0003§>, 0, "2", <§$0003§>)
      pour tout Dossier sur Libellé
      	
      	// Ajoute le dossier
      	nIndiceDossier = RemplitArbre_Dossier(dossier.IDDossier)
      		
      	// Parcourt les flux du dossier
      	pour tout fluxrss sur Titre avec "IDDossier = "+Dossier.IDDossier
      		// Ajoute le flux
      		RemplitArbre_Flux(FluxRss.IDFluxRSS)		
      	FIN
      	
      	// Le noeud doit-il être enroulé ou déroulé ?
      	si optionlit(OPTION_IHM_NOEUDSDEROULES) [~] PREFIX_DOSSIER+Dossier.IDDossier ALORS
      		tablederoule(TABLEH_Flux, nIndiceDossier)
      	SINON
      		tableenroule(TABLEH_Flux, nIndiceDossier)
      	FIN
      	
      FIN
      
      // Parcourt les flux sans dossier
      POUR TOUT FluxRSS SUR Titre AVEC "IDDossier = 0"
      	// Recherche le flux dans la liste
      	RemplitArbre_Flux(FluxRSS.IDFluxRSS)
      FIN
      
      // Déroule la branche "Abonnements"
      TableDéroule(TABLEH_Flux, nIndice)
      
      // Tags
      RemplitArbre_Tags()
      
      // Sélectionne le noeud qui était sélectionné lors de la fermeture précédente
      sNoeud est une chaine = optionlit(OPTION_IHM_NOEUDCOURANT)
      // Si aucun noeud n'était sélectionné en fermeture (1er lancement par exemple)
      si sNoeud = "" alors
      	// Récupère l'indice de la ligne d'abonnement racine
      	nIndice = tablecherche(col_id, idabonnement)
      SINON
      	// Récupère l'indice de la ligne
      	nIndice = tablecherche(col_id, sNoeud)
      	// Récupère l'indice de la ligne d'abonnement racine si besoin
      	si nIndice <= 0 alors nIndice = TableCherche(COL_ID, IDABONNEMENT)
      Fin
      // Sélectionne la ligne
      TableSelectPlus(TABLEH_Flux, nIndice)
      // Applique l'affichage
      ExécuteTraitement(tableh_flux, trtSélection)
      
      // Initialise les compteurs
      ComptageInitialise()
      // Applique le comptage sur l'IHM
      ComptageApplique()
