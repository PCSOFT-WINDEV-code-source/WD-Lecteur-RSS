      // Résumé : Personnalise le menu
      // Syntaxe :
      //Tags_CréationOptionsMenu ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE Tags_CréationOptionsMenu()
      
      nIndice est un entier
      
      
      // Il faut supprimer les Tags précédents
      nIndice = 1
      sOption est une chaine =  OPTIONTAGS+nIndice
      TANTQUE MenuExiste(MENU_Billet_Tag, sOption)
      	// Supprime l'option
      	MenuSupprime(sOption)
      	// Option suivante
      	nIndice++
      	sOption =  OPTIONTAGS+(nIndice)
      FIN
      
      // Ajoute tous les tags mémorisés
      nIndice = 1
      pour TOUT Tag sur libellé
      	
      	// Ajoute le tag
      	menuajouteoption(MENU_Billet_Tag,  OPTIONTAGS+nIndice, Tag.libellé, Tags_Coche, tag.idtag)
      	nIndice++
      	
      FIn
