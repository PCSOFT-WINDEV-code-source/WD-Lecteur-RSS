     p_codes :
      -
        code : |1+
         nIDDossier est un entier sur 8
         nIndice est un entier
         
         // Si la sélection courante est un flux
         si estnumerique(tableh_flux.col_id[tableh_flux]) ALORS
         	
         	// Récupère le dossier parent
         	nIndice = TableRécupèreParent(TABLEH_Flux, TABLEH_Flux)
         	// S'il s'agit d'un dossier
         	si tableh_flux.col_ID[nIndice] [= PREFIX_DOSSIER ALORS
         		// Récupère son identifiant
         		nIDDossier = Val(ExtraitChaîne(TABLEH_Flux.COL_ID[nIndice], 2, "_"))
         	SINOn
         		// Sinon, il s'agit du dossier "Abonnements"
         		nIDDossier = 0
         	FIN
         	
         SINon // Si la sélection courante n'est pas un flux
         	
         	// Récupère l'identifiant du dossier (courant ou parent)
         	SELON TABLEH_Flux.COL_ID[TABLEH_Flux]
         		// Dossier "Abonnements"
         		CAS IDABONNEMENT : nIDDossier = 0
         			
         		// Dossier de flux
         		CAS [= PREFIX_DOSSIER : nIDDossier = Val(ExtraitChaîne(TABLEH_Flux.COL_ID[TABLEH_Flux], 2, "_"))
         			
         		// Autres dossiers (tags, veille, ...)
         		AUTRES CAS : nIDDossier = 0
         			
         	FIN
         	
         FIN
         
         // Ouvre la fenêtre de saisie d'un nouveau flux
         nIDFlux est un entier sur 8
         nIDFlux = Ouvre(FEN_NouveauFlux, nIDDossier)
         si nIDFlux > 0 ALORS
         	// Met à jour le flux
         	majlance([nIDFlux], faux)
         FIN
        type : 18
      -
        type : 161
        enabled : false
