     code : |1-
      // Résumé : Remplit les tags
      // Syntaxe :
      //RemplitArbre_Tags ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE RemplitArbre_Tags()
      
      nIndiceRacine est un entier
      sIDSélectionné est une chaine
      
      // Seulement si les tags sont actifs
      si optionlit(OPTION_IHM_TAGACTIF) = faux alors retour
      
      // Si la sélection courante est un tag ou la racine des tags
      si TABLEH_Flux.COL_ID [= PREFIX_TAG _ou_ TABLEH_Flux.col_id = idtags ALORS
      	// Mémorise l'identifiant sélectionné
      	sIDSélectionné = TABLEH_Flux.COL_ID
      FIN
      
      // Définit l'image
      TABLEH_Flux..ImageDéroulée = IMG_TAG_Arbre
      TABLEH_Flux..ImageEnroulée = IMG_TAG_Arbre
      
      // Recherche la racine des tags
      nIndiceRacine = tablecherche(TABLEH_Flux.COL_ID, IDTAGS)
      si nIndiceRacine <= 0 ALORS
      	// Ajoute la racine si nécessaire
      	nIndiceRacine = tableajouteligne(TABLEH_Flux, IDTags, <§$0004§>, 0, "3", <§$0004§>)
      FIN
      
      // Vide les fils
      TableSupprimeFils(TABLEH_Flux, nIndiceRacine)
      
      // Ajoute les fils
      POUR TOUT Tag SUR Libellé
      	// Ajoute le tag
      	TableAjouteFils(TABLEH_Flux, nIndiceRacine, PREFIX_TAG+Tag.IDTag, Tag.Libellé, 0, Tag.Libellé, Tag.Libellé)
      	// Met à jour le compteur
      	ComptageApplique_UnIdentifiant(PREFIX_TAG+tag.idtag)
      FIN
      
      // Déroule le noeud
      TableDéroule(TABLEH_Flux, nIndiceRacine)
      
      // Repositionne la sélection précédente
      si sIDSélectionné <> "" ALORS
      	nIndice est un entier = tablecherche(tableh_flux.col_id, sIDSélectionné)
      	si nIndice > 0 ALORS
      		tableselectplus(TABLEH_Flux, nIndice)
      	FIN
      FIN
