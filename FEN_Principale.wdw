         si TABLE_Billets[IndiceEnCours].COL_ImageFlux ~= "" alors table_billets[indiceencours].COL_ImageFlux = FluxRSS_RécupèreImageCache(col_idflux)
         
         // Tags associés
         TABLE_Billets[IndiceEnCours].COL_TagsAssociés = Billet_ListeTagsAssociés(TABLE_Billets[IndiceEnCours].col_idbillet)
         
         // Boutons :
         // -  Favoris
         tABLE_Billets[IndiceEnCours].COL_ImageFavoris = (TABLE_Billets[IndiceEnCours].col_favoris ? IMG_FavorisPlein SINON IMG_Favoris)
         tABLE_Billets[IndiceEnCours].COL_Titre..Couleur = (TABLE_Billets[IndiceEnCours].col_favoris ? htmlversrvb("#E91E63") SINON noir)
         // - Partage
         TABLE_Billets[indiceencours].col_partage = IMG_Partage
         // - Tags
         TABLE_Billets[indiceencours].col_tag = (TABLE_Billets[IndiceEnCours].COL_TagsAssociés ~= "" ? IMG_TAg SINON IMG_TAG_Bleu)
