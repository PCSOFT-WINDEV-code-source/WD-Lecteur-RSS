     name : FluxRSS_MAJ_Image
     procedure_id : 1590862560980421012
     type_code : 14
     code : |1-
      // Résumé : Met à jour l'image d'un flux dans l'IHM
      // Syntaxe :
      //FluxRSS_MAJ_Image (<nIDFluxRSS> est entier sur 8 octets)
      //
      // Paramètres :
      //	nIDFluxRSS (entier sur 8 octets) :Identifiant du flux RSS
      // Valeur de retour :
      // 	Aucune
      //
      
      PROCEDURE FluxRSS_MAJ_Image(LOCAL nIDFluxRSS est un entier sur 8)
      
      nIndice est un entier
      nPosition est un entier
      sImageFlux est une chaine
      
      nPosition = hsauveposition(FluxRSS, hSauveRubriques)
      
      // Se positionne sur le flux
      HLitRecherchePremier(FluxRSS, IDFluxRSS, nIDFluxRSS)
      si htrouve(fluxrss) = faux alors retour
      
      // Recherche le flux dans la liste
      nIndice = RemplitArbre_Flux(nIDFluxRSS)
      // L'indice doit être valide
      si nIndice <= 0 alors retour
      
      // Image par défaut
      sImageFlux = FluxRSS_RécupèreImageCache(nIDFluxRSS)
      // Modifie l'image
      TABLEH_Flux[nIndice]..ImageDéroulée = sImageFlux
      TABLEH_Flux[nIndice]..ImageEnroulée = sImageFlux
      
      
      fin:
      hretourposition(nPosition)
     type : 458752
   -
     name : FluxRSS_Affiche
     procedure_id : 1591869683409409391
     type_code : 14
     code : |1+
      // Résumé : Affiche un flux RSS dans la liste
      // Syntaxe :
      //FluxRSS_Affiche (<nIDFluxRSS> est entier sur 8 octets)
      //
      // Paramètres :
      //	nIDFluxRSS (entier sur 8 octets) :Identifiant du flux RSS
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE FluxRSS_Affiche(LOCAL nIDFluxRSS est un entier sur 8)
      
      nIndiceDossier est un entier
      nIndiceRacine est un entier
      sImageFlux est une chaine
      nIndiceFlux est un entier
      
      // Lit le flux RSS
      HLitRecherchePremier(fluxrss, IDFluxRSS, nIDFluxRSS)
      si htrouve(fluxrss) = faux alors retour
      
      // Mémorise l'indice de la racine (Abonnement)
      nIndiceRacine = TableCherche(TABLEH_Flux.COL_ID, IDABONNEMENT)
      
      // Recherche le dossier du flux
      si fluxrss.IDDossier = 0 ALORS
      	// Si aucun dossier, le flux est à la racine
      	nIndiceDossier = nIndiceRacine
      SINON
      	
      	// Ajoute le dossier
      	nIndiceDossier = RemplitArbre_Dossier(fluxrss.IDDossier)
      	
      FIN
      
      // L'indice du dossier parent doit être valide
      SI nIndiceDossier <= 0 ALORS retour
      
      // Récupère l'image du flux
      sImageFlux = FluxRSS_RécupèreImageCache(FluxRSS.IDFluxRSS)
      
      // Mémorise l'indice courant du flux
      nIndiceFlux = RemplitArbre_Flux(fluxrss.IDFluxRSS)
      
      // Modifie le libellé et l'image
      TABLEH_Flux[nIndiceFlux].COL_Libellé = fluxrss.titre
      TABLEH_Flux[nIndiceFlux].COL_LibelléOriginal = fluxrss.titre
      TABLEH_Flux[nIndiceFlux].COL_Tri = "1"+fluxrss.titre
      TABLEH_Flux[nIndiceFlux]..ImageDéroulée = sImageFlux
      TABLEH_Flux[nIndiceFlux]..ImageEnroulée = sImageFlux
      
      // Trie la table
      tabletriefils(TABLEH_Flux, nIndiceDossier, "COL_Tri")
