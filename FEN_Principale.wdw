        internal_properties : CAAAAAgAAABrTXKJweYuBk/cb+qEz6ApQ53c7MiNc3tfUlNguKUPnyHMDrgDoDG9eDMTpKiGDV7X+ooAIHTaHOYlkEX4eAZc1Slcgx1UcDAMlkqk7rqAErOBYHsS0/6fKHA1TK9J/yx97c9ZeoPCMiS2Vww=
        code : |1-
         // Récupère l'indice du clic
         nIndiceClic est un entier = TableInfoXY(TABLEH_Flux,tiNumLigne, SourisPosX(), sourisposy())
         si nIndiceClic <= 0 alors retour
         
         // Sélectionne la ligne cliquée
         tableselectplus(TABLEH_Flux, nIndiceClic)
         
         // Selon la position du clic, le menu à ouvrir diffère
         selon COL_ID[nIndiceClic]
         	// C'est un dossier
         	CAS [= PREFIX_DOSSIER
         		// Ouvre le menu contextuel de dossier
         		ouvremenucontextuel(MENU_Dossier)
         	
         	// C'est la racine
         	CAS IDABONNEMENT
         		// Ouvre le menu contextuel de la racine
         		ouvremenucontextuel(MENU_Abonnements)
         	
         	// Dossier "veille"
         	CAS IDVEILLE
         	
         	// Veille prédéfinie		
         	CAS IDNONLUS, IDFAVORIS, IDAUJOURDHUI, IDHIER
         						
         	// Tags		
         	CAS [= PREFIX_TAG, IDTAGS 			
         		// Ouvre le menu contextuel des tags
         		ouvremenucontextuel(MENU_Tags)
         	
         	// C'est un flux
         	AUTRE CAS
         		// Ouvre le menu contextuel des flux
         		OuvreMenuContextuel(MENU_Flux)
         		
         FIN
