    options :
     -
       name : OPT_Billet_OuvrirNavigageur
       identifier : 0x261780430712a8fb
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           code : |1+
            
            // Parcourt les lignes sélectionnées
            pour toute ligne selectionnée nIndice de table_billets
            	
            	// Lance le navigateur par défaut sur l'adresse du billet
            	Billet_OuvreDansNavigateur(TABLE_Billets.COL_IDBillet[nIndice])
            	
            FIN
           type : 32
     -
       name : OPT_Séparateur5
       identifier : 0x261780470713b8c9
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes : []
     -
       name : OPT_Billet_CopierRaccourci
       identifier : 0x2617800f0709de29
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           code : |1+
            sLiens est une chaine
            
            // Parcourt les lignes sélectionnées
            POUR TOUTE LIGNE SELECTIONNÉE nIndice de table_billets
            	
            	// Récupère le billet
            	stBilletCourant est un stbillet = TABLE_Billets[nIndice]
            	
            	// Mémorise le lien du billet
            	sLiens += [RC] + stBilletCourant.sLien
            	
            FIN
            
            // Aucun lien, ne rien faire
            si sLiens ~= "" alors retour
            
            // Copie les liens des billet dans le presse-papiers
            VersPressePapier(sLiens)
            
            // Prévient l’utilisateur
            sMessage est une chaine
            si TableSelectOccurrence(TABLE_Billets) = 1 ALORS
            	sMessage = <§$004e§>
            SINON
            	sMessage = <§$004f§>
            FIN
            ToastAffiche(<§$002f§>+RC+sMessage, toastCourt, cvMilieu, chCentre)
           type : 32
     -
       name : OPT_Séparateur7
       identifier : 0x263b34204e5116a0
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes : []
     -
       name : OPT_Billet_Archiver
       identifier : 0x263b34054e4bae32
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           code : |1+
            // Au moins une ligne doit être sélectionnée
            si TableSelectOccurrence(TABLE_Billets) <= 0 alors retour
            
            // Définit la question selon le nombre de lignes sélectionnées
            sQuestion est une chaine
            selon TableSelectOccurrence(TABLE_Billets)
            	CAS 1 : sQuestion = <§$004a§>
            	AUTRE CAS : sQuestion = <§$0050§>
            FIN
            
            // Confirmation
            SI Dialogue(sQuestion+RC+<§$0051§>, [<§$004c§>, <§$002e§>], 1, 2, dlgIcôneQuestion) = 2 ALORS RETOUR
            
            bModification 	est un booleen
            bArchivé 		est un booleen
            sRaison 		est une chaine
            sListeRaisons	est une chaine
            // Parcourt les lignes sélectionnées
            POUR TOUTE LIGNE SELECTIONNÉE nIndice de table_billets
            	
            	// Lance l'archivage du billet
            	(bArchivé, sRaison) = Archivage_Billet(TABLE_Billets.COL_IDBillet[nIndice])
            	SI bArchivé ALORS
            		// Au moins un archivage a eu lieu
            		bModification = Vrai
            	SINON
            		// Mémorise la raison de l'échec pour l'afficher à l'utilisateur
            		sListeRaisons += [RC] + chaineconstruit("- ""%1"" : %2",TABLE_Billets.COL_Titre[nIndice], sRaison)
            	FIN
            	
            FIN
            
            // Si un  billet est archivé, il faut rafraîchir l'affichage
            si bModification alors 
            	// Il faut mettre à jour tous les compteurs
            	ComptageInitialise()
            	// Met à jour l'affichage
            	FEN_Principale.ComptageApplique(0,1)
            	AfficheBilletsSélection()
            FIN
            
            // S'il y a eu des échecs, il faut le signaler à l'utilisateur
            si sListeRaisons <> "" ALORS Info(<§$0052§>, sListeRaisons)
           type : 32
