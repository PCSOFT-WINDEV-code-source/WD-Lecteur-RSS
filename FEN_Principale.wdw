     internal_properties : CAAAAAgAAAAb2PUFNkmNSTx2xx+sHAepkw1JvxoWcNmqlRzt1AlWOyAeNAKaPDHzl8ugoFs2zjG7IWn77ZXx1Z4YcA0lGkfEkMnbOIz3SKnR9woJFQwGV2pekNz6V8BR6usOnMOqAqTJP3Pcuo49z4wCMJXgT6uTedyhcx+ISQjQuoWrX6GgPG0gUZsyU39UJJjU2cADBid8EcWWCW3Xcg2zeBFSCmA=
     procedure_id : 1606818583381978681
     type_code : 14
     code : |1-
      // Résumé : Affiche le billet courant dans l'ActivX
      // Syntaxe :
      // AfficheBillet ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE AfficheBillet()
      
      HTM_Apercu = ""
      
      // Modèle utilisé pour uniformiser le style de l'aperçu
      sModele est une chaine = [
      	<HTML>
      	<STYLE>
      	body {
      	font-size: small;
      	font-family: "Segoe UI", "Trebuchet MS", Tahoma, sans-serif;
      	margin: 14px 18px;
      	color: black;
      	background-color: #fff;
      	}
      	</STYLE>
      	<BODY>
      	%1
      	</BODY>
      	</HTML>
      ]
      
      // Il faut contrôler le contenu (et notamment les iframe)
      sContenuBillet est une chaine = billet.contenu
      sURLSite est une chaine
      hlitrecherchepremier(fluxrss, idfluxrss, billet.idfluxrss)
      si htrouve(fluxrss) ALORS sURLSite = fluxrsS.Lien
      // Il faut traiter les iframe (le "src") et les "script"
      sContenuBillet = AfficheBillet_CorrigeContenu(sContenuBillet, sURLSite)
      
      
      // Déclare un fichier temporaire pour l'aperçu
      sFichierApercu est une chaine = COL_Globales.gsRépertoireAperçus +["\"]+ chaineconstruit(<§$006e§>, billet.IDBillet)
      // Crée le fichier s'il n'existe pas
      SI fFichierExiste(sFichierApercu) = faux ALORS
      	// Sauve le contenu pour l'aperçu
      	fSauveTexte(sFichierApercu, ChaîneConstruit(sModele, sContenuBillet))
      Fin
      // Affiche le contenu du billet
      HTM_Apercu = sFichierApercu
     type : 458752
   -
     name : AfficheBillet_CorrigeContenu
     internal_properties : 
     procedure_id : 1606818922684474237
     type_code : 14
     code : |1-
      // Résumé : Corrige le contenu d'un billet pour éviter les soucis d'affichage
      // Syntaxe :
      //[ <Résultat> = ] AfficheBillet_CorrigeContenu (<sContenuBillet> est chaîne, <sURLSite> est chaîne)
      //
      // Paramètres :
      //	sContenuBillet (chaîne ANSI) : Contenu du billet
      //	sURLSite (chaîne ANSI) : URL du site correspondant (pour dé relativiser les images si besoin)
      // Valeur de retour :
      // 	chaîne ANSI : Contenu corrigé
      //
      PROCEDURE PRIVEE AfficheBillet_CorrigeContenu(LOCAL sContenuBillet est une chaine, LOCAL sURLSite est une chaine) : chaine
      
      nDebutBalise, nFinBalise sont des entiers
      nDebutSRC, nFinSRC sont des entiers
      sContenuBilletCorrigé est une chaine
      sSRC est une chaine
      
      
      // Mémorise le contenu initial
      sContenuBilletCorrigé = sContenuBillet
      
      // Supprime les scripts
      sContenuBilletCorrigé = RemplaceJoker(sContenuBilletCorrigé, "<script*/script>", "", sanscasse)
      // Supprime les iframe
      sContenuBilletCorrigé = RemplaceJoker(sContenuBilletCorrigé, "<iframe*/iframe>", "", SansCasse)
      
      // Correction des attributs src sur les images
      tabBalises est un tableau de chaines = ["img"]
      pour tout element sUneBalise de tabBalises
      	
      	// Redéfinit les URL des images dont le chemin est relatif
      	nDebutBalise = Position(sContenuBilletCorrigé, "<"+sUneBalise, 1, SansCasse)
      	TANTQUE nDebutBalise > 0
      		// Cherche la fermeture de la balise
      		nFinBalise = Position(sContenuBilletCorrigé, ">", nDebutBalise)
      		// Pas de fermeture, il faut stopper
      		SI nFinBalise = 0 ALORS SORTIR
      		// Extrait la balise iframe complète
      		sImage est une chaîne = sContenuBilletCorrigé[[nDebutBalise A nFinBalise]]
      		
      		// Recherche le src
      		nDebutSRC = Position(sImage, "src=""",1, SansCasse)
      		// Si le src est trouvé
      		SI nDebutSRC > 0 ALORS
      			nDebutSRC += 5
      			// Recherche la fin du lien
      			nFinSRC = Position(sImage, """", nDebutSRC)
      			// Si la fin est trouvé
      			SI nFinSRC > 0 ALORS
      				nFinSRC--
      				// Extrait le lien de l'attribut src
      				sSRC = sImage[[nDebutSRC A nFinSRC]]
      				
      				// Si le lien débute par "//", force le http (autrement l'ActiveX ne trouvera pas)
      				SI sSRC [= "//" ALORS
      					// Ajoute le http:
      					sSRC = "http:"+sSRC
      					// Corrige le lien dans l'iframe
      					sImage[[nDebutSRC A nFinSRC]] = sSRC
      					// Corrige l'iframe dans le contenu
      					sContenuBilletCorrigé[[nDebutBalise A nFinBalise]] = sImage
      				SINON
      					// Si le lien débute par un simple "/", il faut mettre le chemin en absolu
      					SI sSRC [= "/" ALORS
      						// Ajoute le http:
      						sSRC = sURLSite+["/"]+sSRC
      						// Corrige le lien dans l'iframe
      						sImage[[nDebutSRC A nFinSRC]] = sSRC
      						// Corrige l'iframe dans le contenu
      						sContenuBilletCorrigé[[nDebutBalise A nFinBalise]] = sImage
      					FIn
      				FIN
      				
      			FIN
      			
      		FIN
      		
      		// Recherche une autre balise <img dans le contenu
      		nDebutBalise = Position(sContenuBilletCorrigé, "<"+sUneBalise, nFinBalise, SansCasse)
      		
      	FIN
      	
      FIN
      
      
      // Renvoie le billet corrigé
      renvoyer sContenuBilletCorrigé
