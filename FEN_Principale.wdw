            // Récupère l'identifiant du flux courant
            nIDFluxRSS est un entier sur 8 = TABLEH_Flux.COL_ID[TABLEH_Flux]
            // Récupère l'identifiant du dossier du flux
            nIDDossier est un entier sur 8
            si tableh_flux.col_ID[TableRécupèreParent(TABLEH_Flux)] <> IDABONNEMENT ALORS
            	nIDDossier = val(extraitchaine(TABLEH_Flux.COL_ID[TableRécupèreParent(TABLEH_Flux)], 2, "_"))
            FIN
            
            // Ouvre la fenêtre de choix de dossier
            si ouvre(fen_flux_ChoixDossier, nIDFluxRSS, nIDDossier) ALORS
            	
            	// Supprime la ligne courante (le flux a été déplacé)
            	tablesupprime(TABLEH_Flux)
            	
            	// Met à jour l'affichage du flux
            	FluxRSS_Affiche(nIDFluxRSS)
            	// Applique le comptage pour chaque identifiant
            	ComptageApplique_UnIdentifiant(nIDFluxRSS, 0)	
            	
            	// Cherche le flux
            	nIndice est un entier = tablecherche(tableh_flux.col_id, nIDFluxRSS)
            	si nIndice <= 0 alors retour
            	// Sélectionne le flux
            	tableselectplus(TABLEH_Flux, nIndice)
            	// Met à jour l'IHM
            	executetraitement(TABLEH_Flux, trtselection)
            		
            FIN
