     name : SuppressionEvénement
     internal_properties : CAAAAAgAAACGxzfgFySINZpsbbQjdScQYKfCnyIxPVz1KXSorI0IRWKN7dMzNd08rejdpRyU9rdYfYHiinrsU+IYlRnQqwqN8JbmXzknFslWkYe89OsgtbqXDhUKjSIH1aWiORQLWKfyj9HUy5hZjPVwojld
     procedure_id : 1655725784043451715
     type_code : 14
     code : |1+
      // Résumé : Procédure événement sur une suppression de flux
      // Syntaxe :
      //SuppressionEvénement (<nMessage>, <wParam>, <lParam>)
      //
      // Paramètres :
      //	nMessage : Message
      //	wParam : Contient l'identifiant d'un flux à mettre à jour, 0 pour mettre à jour tous les compteurs
      //	lParam : Non utilisé
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE SuppressionEvénement(nMessage <utile>, wParam, lParam <utile>)
      
      // Mémorise l'identifant du flux
      nIDFlux est un entier sur 8 = wParam
      
      // Recherche la ligne du flux
      nIndice est un entier = tablecherche(TABLEH_Flux.COL_ID, nIDFlux)
      si nIndice <= 0 alors retour
      
      // Supprime la ligne du flux
      TableSupprime(TABLEH_Flux, nIndice)
      
      // Met à jour l'affichage
      AfficheBilletsSélection()
      ComptageInitialise()
      ComptageApplique()
     type : 458752
   -
     name : VérifieFlux
     procedure_id : 1664668816616239681
     type_code : 14
     code : |1+
      // Résumé : Vérifie la validité des flux mémorisés
      // Syntaxe :
      // VérifieFlux ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE VérifieFlux()
      
      tabTâchesVérification est un tableau de TâchesParallèles
      nIDFlux est un entier sur 8
      bRetour est un booleen
      sRetour est une chaine
      tabIDEnErreurs est un tableau d'entiers sur 8
      sListeErreurs est une chaine
      
      // Pour chaque flux, il faut lancer une tâche de vérification
      POUR TOUT FluxRSS 
      	
      	// Mémorise la tâche dans un tableau
      	ajoute(tabTâchesVérification, TâcheParallèleExécute(TP_rssVérifieFlux, (fluxrss.idfluxrss)))
      	
      FIN
      
      // Attend la fin des tâches
      TâcheParallèleAttendToutes(tabTâchesVérification)
      
      // Parcours les tâches
      pour tout element MaTâche de tabTâchesVérification
      	
      	// Récupère les valeurs de retour
      	(nIDFlux, bRetour, sRetour) = MaTâche.ValeurRenvoyée
      	// Si le flux est en erreur
      	si bRetour = faux ALORS
      		HLitRecherchePremier(FluxRSS, IDFluxRSs, nIDFlux)
      		si htrouve(fluxrss) ALORS
      			sListeErreurs += [RC+RC] + chaineconstruit(" - %1 (%2)"+RC+"%3", fluxrss.Titre, FluxRSS.Adresse, sRetour)
      			// Mémorise le flux pour le supprimer si besoin
      			ajoute(tabIDEnErreurs, nIDFlux)
      		FIN
      	FIN
      	
      FIN
      
      // Si le tableau est vide, ne rien faire de plus
      si tabIDEnErreurs..Occurrence = 0 alors retour
      
      // Copie la liste des erreurs dans le presse-papiers
      VersPressePapier(sListeErreurs)
      
      // Affiche les flux en erreurs
      SI Dialogue(<§$0068§>+RC+RC+sListeErreurs+rc+RC+<§$0069§>+rc+RC+<§$006a§>, [<§$006b§>, <§$006c§>], 1, 2, dlgIcôneQuestion) = 1 ALORS
      	// Supprime les flux en erreurs
      	pour tout element nIDFlux de tabIDEnErreurs
      		TâcheParallèleExécute(TP_rssSupprimeFlux, (nIDFlux))
      	Fin
      FIN
     type : 458752
