         si ATT_Image ~= "" alors
         	att_image = Billet_RécupèreImageCache(ATT_IDBillet)
         FIN
         
         // Informations complémentaire
         si ATT_InfosComplémentaire ~= "" ALORS
         	// Si seul un flux est affiché, on n'affiche pas le nom du flux dans les informations complémentaires
         	si estnumerique(TABLEH_Flux.COL_ID[TABLEH_Flux]) ALORS
         		ATT_InfosComplémentaire = chaineconstruit(<§$0047§>, ATT_AuteurSource, dateverschaine(ATT_DatePublicationSource, <§$0048§>))
