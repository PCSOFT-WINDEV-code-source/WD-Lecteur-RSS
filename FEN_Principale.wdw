     name : Tags_Ajoute
     internal_properties : CAAAAAgAAAA0j3DGSbD6ccbmS2Wq6svu88CUvMJ/ea70/mAVVaAiv5IHgZypFrQ1zOSG6HaS1dr4wMRFJrJh4wQgwS7MiLxx45SqkqQeo4Iags5u2EECafqH/clx+u98GLXwKu/oqtPfNkXHeF0hytxfOgHoDF0EoYeDVqxBJDs0uTjiYdVr2rJsLpbT4kEcwBWHhTh4QaMlfplUz8Q=
     procedure_id : 1591939223230232953
     type_code : 14
     code : |1+
      // Résumé : Ajoute un tag pour un billet
      // Syntaxe :
      //Tags_Ajoute (<sTag> est chaîne)
      //
      // Paramètres :
      //	sTag (chaîne ANSI) : Tag à ajouter
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE Tags_Ajoute(LOCAL sTag est une chaine)
      
      // Mémorise l'identifiant du billet
      nIDBillet est un entier sur 8 = gnIDBilletCourant
      si gnIDBilletCourant <= 0 alors retour
      
      // Recherche le tag
      HLitRecherchePremier(tag, libellé, sTag)
      si htrouve(tag) = faux ALORS
      	hraz(tag)
      	tag.Libellé = sTag
      	hajoute(tag)
      FIN
      
      // Ajoute ou supprime l'association
      HLitRecherchePremier(Association, IDAssociation, [nIDBillet, Tag.IDTag])
      SI HTrouve(Association) ALORS
      	HSupprime(Association)
      SINON
      	HRAZ(Association)
      	Association.IDBillet 	= nIDBillet
      	Association.IDTag 		= Tag.IDTag
      	HAjoute(Association)
      FIN
      
      // Force le remplissage de l'arbre (pour ajouter le nouveau tags)
      RemplitArbre_Tags()
      // Reforce la création du menu originale (un tag supplémentaire)
      Tags_CréationOptionsMenu()
      
      // Met à jour le compteur pour ce tag
      MAJ_Compteur_Tag(tag.IDTag)
      ComptageApplique_UnIdentifiant(PREFIX_TAG+tag.idtag, Faux)
      
      
      // Rafraîchit l'affichage de la ligne
      nIndice est un entier
      sTagsAssociés est une chaine = Billet_ListeTagsAssociés(gnIDBilletCourant)
      
      nIndice = tablecherche(COL_IDBillet, gnIDBilletCourant)
      si nIndice > 0 alors COL_TagsAssociés[nIndice] = sTagsAssociés
      
      nIndice = zonerepeteeCherche(ATT_IDBillet, gnIDBilletCourant)
      SI nIndice > 0 ALORS ATT_TagsAssociés[nIndice] = sTagsAssociés
