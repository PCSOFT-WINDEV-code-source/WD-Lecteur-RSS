        code : |1+
         // Récupère la colonne survolée
         sColonne est une chaine = TableInfoXY(MoiMême, tiNomColonne, SourisPosX(), sourisposy())
         // Récupère la ligne survolée
         nIndice est un entier = TableInfoXY(MoiMême, tinumligne, SourisPosX(), sourisposy())
         si nIndice <= 0 alors retour
         // Si on survole la même ligne et même colonne, ne rien faire
         si nIndice = gnIndiceSurvol _et_ sColonne = gsColonneSurvol alors retour
         // Mémorise la ligne et la colonne
         gnindicesurvol = nIndice
         gsColonneSurvol = sColonne
         
         // Adapte la bulle à la colonne
         selon sColonne
         	// Favoris
         	CAS COL_ImageFavoris..nom
         		MoiMême..Bulle = (COL_Favoris[nIndice] ? <§$0066§> SINON <§$0067§>)
         		
         	// Partage
         	CAS COL_Partage..nom
         		MoiMême..Bulle = <§$0045§>
         		
         	// Tags
         	cas COL_Tag..nom
         		moimeme..bulle = <§$0046§>
         		// Affiche les tags déjà affectés
         		si TABLE_Billets[nIndice].COL_TagsAssociés <> "" alors
         			moimeme..bulle += RC+RC+ gPoliceItalique(Vrai)+gStylo(HTMLVersRVB("#2196F3")) + remplace(TABLE_Billets[nIndice].COL_TagsAssociés, ";", rC)
         		Fin
         		
         	// Autres colonnes
         	AUTRE CAS
         		// Récupère le billet
         		stUnBillet est un stbillet = table_billets[nIndice]
         		// Par défaut, affiche le titre et la description (le début)
         		sDescription est une chaine = HTMLVersTexte(stUnBillet.sDescription)
         		moimeme..bulle = gpolicegras(vrai)+stUnBillet.stitre+gpolicegras(faux) +rc+rc+ gauche(sDescription, 200) + (taille(sDescription) > 200 ? "..." sinon "") +RC+RC+ gpoliceitalique(vrai)+gstylo(HTMLVersRVB("#2196F3"))+TABLE_Billets[nIndice].COL_TagsAssociés
         FIN
