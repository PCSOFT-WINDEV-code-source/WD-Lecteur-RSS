        code : |1-
         
         sNom est une chaine
         
         // Ouvre la fenêtre pour saisir un nouveau dossier
         SaisieNom : 
         SI Saisie(<§$0054§>, sNom, [<§$0055§>, <§$0056§>], 1, 2, dlgIcôneQuestion) = 2 ALORS RETOUR
         
         // Le nom ne doit pas être vide
         si sNom ~= "" ALORS
         	Erreur("Vous devez saisir un nom de dossier valide (non vide)")
         	GOTO SaisieNom
         FIN
         
         // Le dossier existe-t-il déjà ?
         HRecherchePremier(Dossier, Libellé, sNom)
         SI HTrouve(Dossier) ALORS
         	
         	// Message d'erreur
         	ErreurConstruit(<§$0057§>, sNom)
         	
         SINON
         	
         	// Ajoute le dossier en base
         	HRAZ(Dossier)
         	Dossier.Libellé = sNom
         	SI HAjoute(Dossier) = Faux ALORS
         		Erreur(<§$0058§>, HErreurInfo(hErrMessage))
         		RETOUR
         	FIN
         	
         	// Met à jour l'arbre
         	RemplitArbre_Dossier(Dossier.IDDossier)
         	
         	// Trie la branche "Abonnements
         	TableTrieFils(TABLEH_Flux, 1, "COL_Tri")
         	
         FIN
