        internal_properties : CAAAAAgAAAApMYDNdRc1Fo3iv5PvfFBGTW7zyHN8U784lnBfqdZfj/G3SN9ebJ+WjFTnSua3NPsX4I8Mj8gaxFTp+WyqrrTH1C2NziS5QVBQ3Njp1+yn02AzClcIMKuc8T3ECqNK50HQoiySwK+bwh9rJAZukS5YQG5XwPExq2BgMCd8ynD1nSpmxY1LUGwOx8ZQfAhMKzWe5LkfhRXSxHeBJr9TviKORSmrmsSYJuOrOjrWMg==
        code : |1+
         PROCEDURE EnrouléDéroulé()
         
         // Si la gestion automatique des enroulés n'est pas activée, ne rien faire
         si val(optionlit(OPTION_IHM_DEROULERDOSSIERAUTO)) = faux alors retour
         
         // Récupère l'indice qui a été enroulé / déroulé
         nIndice est un entier = MoiMême
         
         // Si le noeud n'est pas déroulé (il a été enroulé), ne rien faire
         si TableEtatElément(moimeme, nIndice) <> taenroule alors retour
         
         nIndiceFils est un entier
         // Mémorise l'indice du noeud "Abonnements"
         nIndiceAbonnement est un entier = TableCherche(COL_ID, IDABOnnement)
         // Parcours les fils direct du noeud "Abonnement"
         nIndiceFils = TableRécupèreFils(MoiMême, nIndiceAbonnement, taPremier)
         TANTQUE nIndiceFils <> -1
         	// S'il ne s'agit pas du noeud qui vient d'être déroulé
         	si nIndiceFils <> nIndice ALORS
         		// Enroule le noeud)
         		tableenroule(moimeme, nIndiceFils)
         	FIN
         	nIndiceFils = TableRécupèreFils(MoiMême, nIndiceAbonnement, taSuivant)
         FIN
        type : 36
      -
        type : 230
        enabled : false
      -
        code : |1-
         // Récupère l'indice du clic
         nIndiceSurvol est un entier = TableInfoXY(TABLEH_Flux,tiNumLigne, SourisPosX(), sourisposy())
         // L'indice doit être valide
         si nIndiceSurvol <= 0 alors
         	moimeme..bulle = ""
         	retour
         FIN
         // L'indice doit être différent du précédent
         si nIndiceSurvol = gnIndiceSurvolListe alors retour
         
         // Mémorise l'indice de survol courant
         gnIndiceSurvolListe = nIndiceSurvol
         
         sBulle est une chaine
         
         // Selon la position du survol, la bulle diffère
         SELON COL_ID[nIndiceSurvol]
         	// C'est un dossier
         	CAS [= PREFIX_DOSSIER : sBulle = chaineconstruit(<§$0031§>, TABLEH_Flux.COL_LibelléOriginal[nIndiceSurvol])
         	
         	// C'est la racine
         	CAS IDABONNEMENT : sBulle = <§$0032§>+RC+<§$0033§>
         		
         	// Dossier "veille"
         	CAS IDVEILLE :  sBulle = <§$0034§>+rc+<§$0035§>
         		
         	// Non lus
         	CAS IDNONLUS : sBulle = <§$0036§>+RC+<§$0037§>
         	
         	// Favoris
         	CAS IDFAVORIS : sBulle = <§$0038§>+RC+<§$0039§>
         		
         	// Aujourd'hui
         	CAS IDAUJOURDHUI : sBulle = <§$003a§>+RC+<§$003b§>
         	
         	// Hier
         	CAS IDHIER : sBulle = <§$003c§>+RC+<§$003d§>
         		
         	// Tags		
         	CAS [= PREFIX_TAG : sBulle = chaineconstruit(<§$003e§>+RC+<§$003f§>, TABLEH_Flux.COL_LibelléOriginal[nIndiceSurvol])
         	
         	// Liste des tags
         	CAS IDTAGS : sBulle = <§$0040§>+RC+<§$0041§>
         
         	// C'est un flux
         	AUTRE CAS
         		sBulle = <§$0042§>+tableh_flux.COL_LibelléOriginal[nIndiceSurvol]
         FIN
         
         // Affecte la bulle
         MoiMême..Bulle = sBulle
