       identifier : 0x261b846a08c23ec6
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           code : |1-
            sFichier est une chaîne
            
            // Ouvre le sélecteur de fichiers
            sFichier = fSélecteur("", "", <§$0002§>, fFiltreImage(), ExtraitChaîne(ExtraitChaîne(fFiltreImage(), 2, TAB), 1, RC))
            si sFichier ~= "" alors retour
            
            // Modifie l'image
            IMG_Image = sFichier
            
