  procedures :
   -
     name : RemplitListe
     procedure_id : 1591971319539187703
     type_code : 14
     code : |1-
      // Résumé : Remplit la liste
      // Syntaxe :
      // RemplitListe ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE RemplitListe()
      
      // Vide la liste
      zonerepeteesupprimetout(ZR_Tags)
      
      // Parcourt les tags existants
      pour TOUT Tag 
      	// Ajoute le tag
      	soit nIndice = zonerepeteeajouteligne(ZR_Tags, tag.idtag, tag.libellé)
      	// Si une association existe
      	hrecherchepremier(Association,IDAssociation,[gnIDBillet, tag.idtag])
      	si htrouve(Association) ALORS
      		// Affiche le tag en gras et coche la ligne
      		ATT_Associé[nIndice] = vrai
      	FIN
      FIN
      
      // Parcourt les suggestions
      pour toute chaine sUneSuggestion de billet.categorie separee par ";"
      	
      	// Si la suggestion n'est pas déjà présente
      	si ZoneRépétéeCherche(ATT_Libellé, sUneSuggestion) <= 0 ALORS
      		// Ajoute la suggestion
      		ZoneRépétéeAjouteLigne(ZR_Tags, 0, sUneSuggestion)
      	FIN
      		
      FIN
      
      // Trie
      zonerepeteetrie("ATT_Libellé")
