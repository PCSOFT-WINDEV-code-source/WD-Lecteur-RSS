      -
        code : |1+
         // Supprime les associations courantes
         pour tout association avec IDBillet = gnidbillet
         	hsupprime(association)
         FIN
         
         // Mémorise les associations
         pour toute ligne nIndice de ZR_Tags
         	// Si le tag courant est marqué comme associé
         	si ATT_Associé[nIndice] ALORS
         		// Si le tag n'existe pas encore
         		si att_idtag[nIndice] <= 0 ALORS
         			// Enregistre le tag
         			hraz(tag)
         			tag.Libellé = ATT_Libellé[nIndice]
         			hajoute(tag)
         			ATT_IDTag[nindice] = tag.idtag
         		FIN
         		// Enregistre l'association
         		hraz(association)
         		Association.IDBillet = gnIDBillet
         		association.IDTag =ATT_IDTag[nIndice] 
         		hajoute(association)
         		
         	FIN
         FIN
         
         
         // Ferme la fenêtre en validation
         ferme("", Vrai)
