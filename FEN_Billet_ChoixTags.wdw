   -
     code : |1+
      PROCEDURE MaFenêtre(gnIDBillet est un entier sur 8)
      
      // Valeur de retour par défaut
      mafenetre..valeurrenvoyee = faux
      
      // Lit le billet
      HLitRecherchePremier(Billet, IDBillet, gnIDBillet)
      SI HTrouve(Billet) = Faux ALORS
      	Erreur(<§$0000§>)
      	Ferme()
      FIN
      
      // Titre de la fenêtre
      mafenetre..titre = <§$0001§>+Billet.Titre
      
      // Texte
      LIB_Explications = chaineconstruit(LIB_Explications, billet.titre)
   -
     code : |1+
      // Remplit la liste des tags
      RemplitListe()
      
