  procedures :
   -
     name : RemplitThèmes
     procedure_id : 1599744789673450536
     type_code : 14
     code : |1-
      // Résumé : Remplit la liste des thèmes
      // Syntaxe :
      // RemplitThèmes ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE RemplitThèmes()
      
      
      // Ajoute les couleurs disponibles
      Ajoute(gtabThème,"#212121")	// Noir
      Ajoute(gtabThème,"#515151")	// Noir (défaut)
      Ajoute(gtabThème,"#BDBDBD")	// Gris
      
      Ajoute(gtabThème,"#E53935")	// Rouge
      Ajoute(gtabThème,"#D81B60")	// Rose
      Ajoute(gtabThème,"#8E24AA")	// Pourpre
      
      Ajoute(gtabThème,"#3949AB")	// Indigo
      Ajoute(gtabThème,"#1E88E5")	// Bleu
      Ajoute(gtabThème,"#00ACC1")	// Cyan
      
      Ajoute(gtabThème,"#43A047")	// Vert
      Ajoute(gtabThème,"#7CB342")	// Vert clair
      Ajoute(gtabThème,"#C0CA33")	// Citron
      
      Ajoute(gtabThème,"#FDD835")	// Jaune
      Ajoute(gtabThème,"#FB8C00")	// Orange
      Ajoute(gtabThème,"#F4511E")	// Orange foncé
      
      // Vide le champ
      zonerepeteesupprimetout(ZR_Thème)
      
      // Remplit les thèmes
      sCouleur est une chaine
      pour tout element sCouleur de gtabThème
      	zonerepeteeajouteligne(ZR_Thème, HTMLVersRVB(sCouleur), sCouleur)
      FIN
      // Supprime la sélection initiale
      zonerepeteeselectmoins(ZR_Thème)
